<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../isisxsl.xsl"?>
<isis lang="en">
    <stitle>TSI 05-12-29 - Connecting Rod Bearing Kits</stitle>
    <tsi id="tsi051229" year="2005" tsitype="engine">
        <tsinumber>05-12-29</tsinumber>
        <tsidate>July 2005</tsidate>
        <subjectfile>Engine</subjectfile>
        <subjectgroup>
            <subject>Connecting Rod Bearing Kits</subject>
        </subjectgroup>
        <apply>
            <configure>
                <engfamgroup/>

                <engfam>VT 275</engfam>
            </configure>
        </apply>
        <description>
            <geninfo>
                <para>International&#174; VT 275 connecting rod bearing kits have been released.</para>
                <specpara>
                    <caution>
                        <para>Connecting rod bearings for International&#174; VT 365 engines should not be used in
                            International&#174; VT 275 engines. International&#174; VT 365 connecting rod bearings may
                            fit and appear to work in International&#174; VT 275 engines, but will cause engine damage.
                        </para>
                    </caution>
                </specpara>
                <table id="t01" tblwidth="col-wide" frame="topbot">
                    <title>International&#174; VT 275 connecting rod bearing kits</title>
                    <tgroup cols="2">
                        <colspec colname="col1" colwidth="1.53*"/>
                        <colspec colname="col2" align="center" colwidth="1.02*"/>
                        <tbody>
                            <row>
                                <entry>
                                    <para>
                                        <emph type="bold">Description</emph>
                                    </para>
                                </entry>
                                <entry align="center">
                                    <para>
                                        <emph type="bold">Part Number</emph>
                                    </para>
                                </entry>
                            </row>
                            <row>
                                <entry>
                                    <para>Rod bearing kit, standard</para>
                                </entry>
                                <entry align="center">
                                    <para>1848131C91</para>
                                </entry>
                            </row>
                            <row>
                                <entry>
                                    <para>Connecting rod bearing, standard</para>
                                </entry>
                                <entry align="center">
                                    <para>1848042C1</para>
                                </entry>
                            </row>
                            <row>
                                <entry>
                                    <para>Rod bearing kit, .254 mm under</para>
                                </entry>
                                <entry align="center">
                                    <para>1848132C91</para>
                                </entry>
                            </row>
                            <row>
                                <entry>
                                    <para>Connecting rod bearing, 0.254 mm</para>
                                </entry>
                                <entry align="center">
                                    <para>1848077C1</para>
                                </entry>
                            </row>
                            <row>
                                <entry>
                                    <para>Rod bearing kit, .508 mm under</para>
                                </entry>
                                <entry align="center">
                                    <para>1848133C91</para>
                                </entry>
                            </row>
                            <row>
                                <entry>
                                    <para>Connecting rod bearing, 0.508 mm</para>
                                </entry>
                                <entry align="center">
                                    <para>1848081C1</para>
                                </entry>
                            </row>
                            <row>
                                <entry>
                                    <para>Rod bearing kit, .762 mm under</para>
                                </entry>
                                <entry align="center">
                                    <para>1848134C91</para>
                                </entry>
                            </row>
                            <row>
                                <entry>
                                    <para>Connecting rod bearing, 0.762 mm</para>
                                </entry>
                                <entry align="center">
                                    <para>1848083C1</para>
                                </entry>
                            </row>
                            <row>
                                <entry>
                                    <para>Connecting rod and cap assembly kit</para>
                                </entry>
                                <entry align="center">
                                    <para>1848169C91</para>
                                </entry>
                            </row>
                        </tbody>
                    </tgroup>
                </table>
                <para>
                    <emph type="bold">Unit conversion</emph>
                </para>
                <para>0.254 mm = 0.010 in</para>
                <para>0.508 mm = 0.020 in</para>
                <para>0.762 mm = 0.030 in</para>
                <para>Connecting rod bearings have the part number stamped on the back.</para>
                <specpara>
                    <note>
                        <para>Verify correct bearing part number before installation (Figure 1).</para>
                    </note>
                </specpara>
                <figure id="f0512291" figsize="col-wide">
                    <graphic filename="../tsifigures/J04010.webp" type="webp" scalefitwidth="264px"/>
                    <caption>Figure 1. International&#174; VT 275 and VT 365 connecting rod bearings showing stamped
                        part numbers
                    </caption>
                    <calloutgroup>
                        <callout1 type="arabic">International&#174; VT 275 connecting rod bearing, standard</callout1>
                        <callout1 type="arabic">International&#174; VT 365 connecting rod bearing, standard</callout1>
                    </calloutgroup>
                </figure>
            </geninfo>
        </description>
    </tsi>
</isis>

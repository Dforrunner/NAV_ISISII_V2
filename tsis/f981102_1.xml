    <stitle>French - TSI 98-11-02 - Mise &#224; jour des leviers d'embrayage</stitle>
    <tsi id="tsi981102" tsitype="truck" translation="french">
        <tsinumber>98-11-02</tsinumber>
        <tsidate>D&#233;cembre 1998</tsidate>
        <subjectfile>EMBRAYAGE</subjectfile>
        <subjectgroup>
            <subject>Mise &#224; jour des leviers d'embrayage</subject>
        </subjectgroup>
        <apply>
            <configure>
                <truckmodelgroup/>

                <truckmodel>9100
                    <daterange>
                        <startdate>09/01/1996</startdate>
                        <enddate>07/03/1998</enddate>
                    </daterange>
                </truckmodel>
                <truckmodel>9200
                    <daterange>
                        <startdate>09/01/1996</startdate>
                        <enddate>07/03/1998</enddate>
                    </daterange>
                </truckmodel>
                <truckmodel>9300
                    <daterange>
                        <startdate>09/01/1996</startdate>
                        <enddate>07/03/1998</enddate>
                    </daterange>
                </truckmodel>
                <truckmodel>9400
                    <daterange>
                        <startdate>09/01/1996</startdate>

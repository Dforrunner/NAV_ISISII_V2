    <stitle>French - TSI 02-12-17 - Tuyau sup&#233;rieur de radiateur pour les moteurs N14 de Cummins</stitle>
    <tsi id="tsi021217" tsitype="truck" translation="french">
        <tsinumber>02-12-17</tsinumber>
        <tsidate>Septembre 2002</tsidate>
        <subjectfile>MOTEUR</subjectfile>
        <subjectgroup>
            <subject>Tuyau sup&#233;rieur de radiateur pour les moteurs N14 de Cummins</subject>
        </subjectgroup>
        <apply>
            <configure>
                <vendorgroup/>

                <vendor>Cummins</vendor>
                <truckmodelgroup/>

                <truckmodel>2500
                    <daterange>
                        <startdate>01/28/2002</startdate>
                    </daterange>
                </truckmodel>
                <truckmodel>2600
                    <daterange>
                        <startdate>01/28/2002</startdate>
                    </daterange>

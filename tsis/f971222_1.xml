<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../isisxslfrench.xsl"?>
<isis lang="fr">
    <stitle>French - TSI 97-12-22 - &#201;limination de la clavette et de la rainure de clavette sur le pignon des
        compresseurs d'air Tu-Flo 550 et Tu-Flo 750
    </stitle>
    <tsi id="tsi971222" tsitype="engine" translation="french">
        <tsinumber>97-12-22</tsinumber>
        <tsidate>Octobre 1997</tsidate>
        <subjectfile>MOTEURS</subjectfile>
        <subjectgroup>
            <subject>&#201;limination de la clavette et de la rainure de clavette sur le pignon des compresseurs d'air
                Tu-Flo 550 et Tu-Flo 750
            </subject>
        </subjectgroup>
        <apply>
            <configure>
                <engfamgroup/>

                <engfam>DT 466</engfam>
                <engfam>DT 466E</engfam>
                <engfam>530</engfam>
                <engfam>530E</engfam>
            </configure>
        </apply>
        <description>
            <geninfo>
                <para>&#192; compter du num&#233;ro de s&#233;rie de moteur 1050304, la clavette sur l'arbre de
                    compresseur d'air et les rainures sur le pignon et le vilebrequin de compresseur d'air sont &#233;limin&#233;es.
                    L'arbre conique avec couple de boulon de 110 lb&#8226;pi (149 N&#8226;m) est suffisant pour que le
                    pignon ne tourne pas sur l'arbre et cela &#233;limine la n&#233;cessit&#233; de la clavette et de la
                    rainure.
                </para>
            </geninfo>
        </description>
        <partsinfo>
            <table id="t9712221" frame="all">
                <title>Renseignement sur les pi&#232;ces</title>
                <tgroup cols="3" colsep="1" rowsep="1">
                    <colspec colwidth="1.75in"/>
                    <colspec colwidth="2.75in"/>
                    <colspec colwidth="1.99in"/>
                    <thead>
                        <row>
                            <entry valign="top" align="center">
                                <para>
                                    <emph type="bold">Nouveau num&#233;ro de pi&#232;ce</emph>
                                </para>
                            </entry>
                            <entry valign="top" align="center">
                                <para>
                                    <emph type="bold">Description</emph>
                                </para>
                            </entry>
                            <entry valign="top" align="center">
                                <para>
                                    <emph type="bold">Num&#233;ro de pi&#232;ce remplac&#233;</emph>
                                </para>
                            </entry>
                        </row>
                    </thead>
                    <tbody>
                        <row>
                            <entry valign="top" align="center">
                                <para>2 003 252 C95</para>
                            </entry>
                            <entry valign="top" align="center">
                                <para>Compreesseur d'air (Tu-Flo 550)</para>
                            </entry>
                            <entry valign="top" align="center">

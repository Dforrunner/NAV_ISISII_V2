    <stitle>Spanish - TSI 07-12-12 - Juegos de retorno de refrigerante del EGR</stitle>
    <tsi id="M071212" year="2007" tsitype="engine" translation="spanish">
        <tsinumber>07-12-12</tsinumber>
        <tsidate>Julio 2007</tsidate>
        <subjectfile>Motor</subjectfile>
        <subjectgroup>
            <subject>Juegos de retorno de refrigerante del EGR</subject>
        </subjectgroup>
        <apply>
            <configure>

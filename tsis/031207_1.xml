<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../isisxsl.xsl"?>
<isis lang="en">
    <stitle>TSI 03-12-07 - DT 466 Service Piston</stitle>
    <tsi id="tsi031207" tsitype="engine">
        <tsinumber>03-12-07</tsinumber>
        <tsidate>June, 2003</tsidate>
        <subjectfile>Engine</subjectfile>
        <subjectgroup>
            <subject>DT 466 Service Piston</subject>
        </subjectgroup>
        <apply>
            <configure>
                <engfamgroup/>

                <engfam>DT 466
                    <snrange>1194039</snrange>
                </engfam>
            </configure>
        </apply>
        <description>
            <geninfo>
                <para>This TSI applies to the DT 466 in 175-215 Horsepower applications (Beginning with 2000 model year
                    and engine serial number 1194039).
                </para>
                <para>In order to improve emission levels and durability, piston 1836321C1 will replace 1830621C2 for
                    service. While the new piston can be installed individually, it is preferable to replace all six.
                </para>
                <table id="t0312071">
                    <title>New and Displaced Piston/ Piston Package Numbers</title>
                    <tgroup cols="3">
                        <colspec colname="COLSPEC0" colwidth="1.08*"/>
                        <colspec colname="col1" colwidth="1.08*"/>
                        <colspec colname="col2" colwidth="0.92*"/>
                        <tbody>
                            <row>
                                <entry>
                                    <para>Description</para>
                                </entry>
                                <entry>
                                    <para>New part number</para>
                                </entry>
                                <entry>
                                    <para>Displaced part number</para>
                                </entry>
                            </row>
                            <row>
                                <entry>
                                    <para>Piston</para>
                                </entry>
                                <entry>
                                    <para>1836321C1</para>
                                </entry>
                                <entry>
                                    <para>1830621C2</para>
                                </entry>
                            </row>
                            <row>
                                <entry>
                                    <para>Piston and sleeve package</para>
                                </entry>
                                <entry>
                                    <para>1836194C92</para>
                                </entry>
                                <entry>
                                    <para>1836194C91</para>
                                </entry>
                            </row>
                            <row>
                                <entry>
                                    <para>Piston, sleeve, and ring package</para>
                                </entry>
                                <entry>
                                    <para>1836192C92</para>
                                </entry>
                                <entry>
                                    <para>1836192C91</para>
                                </entry>
                            </row>
                        </tbody>
                    </tgroup>
                </table>
            </geninfo>
        </description>
    </tsi>
</isis>

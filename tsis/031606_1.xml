<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../isisxsl.xsl"?>
<isis lang="en">
    <stitle>TSI 03-16-06 - Sanden Air Conditioning Compressors used in 5000i and 9000i Models Built After November 1,
        2002
    </stitle>
    <tsi id="tsi031606" tsitype="truck">
        <tsinumber>03-16-06</tsinumber>
        <tsidate>October, 2003</tsidate>
        <subjectfile>CAB</subjectfile>
        <subjectgroup>
            <subject>Sanden Air Conditioning Compressors used in 5000i and 9000i Models Built After November 1, 2002
            </subject>
        </subjectgroup>
        <apply>
            <configure>
                <truckmodelgroup/>

                <truckmodel>5000i
                    <daterange>
                        <startdate>11/01/2002</startdate>
                    </daterange>
                </truckmodel>
                <truckmodel>9000i
                    <daterange>
                        <startdate>11/01/2002</startdate>
                    </daterange>
                </truckmodel>
            </configure>
        </apply>
        <description>
            <geninfo>
                <para>Effective with the change to engines meeting 2002 emission standards, 5000i and 9000i Models built
                    after November 1, 2002 use Sanden air conditioning compressors.
                </para>
                <para>
                    <title>Specifications</title>
                </para>
                <seqlist>
                    <item>
                        <para>AC Compressor Oil: PAG, International Part Number ZGGR6822 or Sanden SP-15</para>
                    </item>
                    <item>
                        <para>System Capacity and Coolant Type: 3 pounds, R-134A</para>
                    </item>
                </seqlist>
            </geninfo>
        </description>
    </tsi>
</isis>

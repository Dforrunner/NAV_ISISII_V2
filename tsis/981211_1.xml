<isis lang="en">
    <stitle>TSI 98-12-11 - Release of 24 Volt International Diamond Logic&#8482; Electronic Control Module</stitle>
    <tsi id="tsi981211" tsitype="truck">
        <tsinumber>98-12-11</tsinumber>
        <tsidate>December, 1998</tsidate>
        <subjectfile>ENGINE</subjectfile>
        <subjectgroup>
            <subject>Release of 24 Volt International Diamond Logic&#8482; Electronic Control Module</subject>
        </subjectgroup>
        <apply>
            <configure>
                <engfamgroup/>

                <engfam>DT 466E</engfam>
                <engfam>530E</engfam>
            </configure>
        </apply>
        <description>
            <geninfo>
                <para>A 24 Volt Electronic Control Module is now available for service for those applications requiring
                    higher voltage.
                </para>
                <specpara>
                    <note>
                        <para>No other changes to the electronic system are required when retrofitting with the 24 Volt
                            Electronic Control Module.
                        </para>
                    </note>
                </specpara>
            </geninfo>
        </description>
        <partsinfo>
            <table id="t9901011">
                <title>Parts Affected</title>
                <tgroup cols="2">
                    <colspec colname="colspec0" colwidth="2.02in"/>
                    <colspec colname="col1" colwidth="3.71in"/>
                    <thead>
                        <row>
                            <entry colname="colspec0" colsep="1" rowsep="1" valign="top" align="center" charoff="50"
                                   char="">
                                <para>
                                    <emph type="bold">New Part Number</emph>
                                </para>
                            </entry>
                            <entry colname="col1" colsep="1" rowsep="1" valign="top" align="center" charoff="50"
                                   char="">
                                <para>
                                    <emph type="bold">Description</emph>
                                </para>
                            </entry>
                        </row>
                    </thead>
                    <tbody>
                        <row>
                            <entry colname="colspec0" colsep="1" rowsep="1" valign="top" align="center" charoff="50"
                                   char="">
                                <para>1 807 350C1</para>
                            </entry>
                            <entry colname="col1">
                                <para>24 Volt Electronic Control Module</para>
                            </entry>
                        </row>
                    </tbody>
                </tgroup>
            </table>
        </partsinfo>
    </tsi>
</isis>

<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../isisxsl.xsl"?>
<isis lang="en">
    <stitle>TSI 06-12-20 - Turbocharger VGT Linkage Kit</stitle>
    <tsi id="tsi061220" year="2006" tsitype="engine">
        <tsinumber>06-12-20</tsinumber>
        <tsidate>September 2006</tsidate>
        <subjectfile>Engine</subjectfile>
        <subjectgroup>
            <subject>Turbocharger VGT Linkage Kit</subject>
        </subjectgroup>
        <apply>
            <configure>
                <engfamgroup/>

                <engfam>DT 466
                    <snrange>2000001 and up</snrange>
                </engfam>
                <engfam>DT 570
                    <snrange>2000001 and up</snrange>
                </engfam>
                <engfam>HT 570
                    <snrange>2000001 and up</snrange>
                </engfam>
            </configure>
        </apply>
        <description>
            <geninfo>
                <para>A turbocharger VGT linkage kit (1876951C91) is available to replace stuck or sticking VGT linkage
                    instead of replacing the turbocharger.
                </para>
                <specpara>
                    <note>
                        <para>This is an intermediate fix, new linkages are being developed utilizing material with
                            better corrosion resistance.
                        </para>
                    </note>
                </specpara>
            </geninfo>
        </description>
    </tsi>
</isis>

    <stitle>French - TSI 97-01-01 - Identification de traverse &#224; goussets en acier embouti</stitle>
    <tsi id="tsi970101" tsitype="truck" translation="french">
        <tsinumber>97-01-01</tsinumber>
        <tsidate>Septembre 1997</tsidate>
        <subjectfile>B&#194;TI</subjectfile>
        <subjectgroup>
            <subject>Identification de traverse &#224; goussets en acier embouti</subject>
        </subjectgroup>
        <apply>
            <configure>
                <truckmodelgroup/>

                <truckmodel>1000</truckmodel>

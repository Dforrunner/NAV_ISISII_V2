<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../isisxsl.xsl"?>
<isis lang="en">
    <stitle>TSI 95-12-11 - Correction to Lubrication Oil Pressure Specifications On DT 408, DT 466 and International&#174;
        530 Diesel Engines
    </stitle>
    <tsi id="tsi951211" tsitype="engine">
        <tsinumber>95-12-11</tsinumber>
        <tsidate>August, 1995</tsidate>
        <subjectfile>ENGINE</subjectfile>
        <subjectgroup>
            <subject>Correction to Lubrication Oil Pressure Specifications On DT 408, DT 466 and International&#174; 530
                Diesel Engines
            </subject>
        </subjectgroup>
        <apply>
            <configure>
                <engfamgroup/>

                <engfam>DT 408</engfam>
                <engfam>DT 466</engfam>
                <engfam>530</engfam>
            </configure>
        </apply>
        <svcproc svcproctype="TSI" graphiccount="0">
            <title>DESCRIPTION</title>
            <svcprocinfo>
                <para>A correction has been made to the lubrication specifications for the DT 408, DT 466 and
                    International&#174; 530 diesel engines. Please make the following changes in your manual.
                </para>
            </svcprocinfo>
            <svcsubproc>
                <title>Was:</title>
                <randlist type="bullet">
                    <item>
                        <para>At Low Idle (Minimum 650&#177;50 rpm) 137 to 344 kPa (20 to 50 psi) minimum</para>
                    </item>
                    <item>
                        <para>At High Idle 552 kPa (80 psi)</para>
                    </item>
                </randlist>
                <para>Engine at normal operating temperature.</para>
            </svcsubproc>
            <svcsubproc>
                <title>Change to the following:</title>
                <randlist type="bullet">
                    <item>
                        <para>At Low Idle (Minimum 650&#177;50 rpm) 104 kPa (15 psi)</para>
                    </item>
                    <item>
                        <para>At High Idle 345 to 497 kPa (50 to 72 psi)</para>
                    </item>
                </randlist>
                <para>Engine at normal operating temperature.</para>
            </svcsubproc>
            <svcsubproc>
                <title>Make this change in the appropriate places in the following manuals:</title>
                <table id="t9512111" frame="all">
                    <title>Change Locations</title>
                    <tgroup cols="2" colsep="1" rowsep="1">
                        <colspec colwidth="2.19in"/>
                        <colspec colwidth="4.29in"/>
                        <tbody>
                            <row>
                                <entry valign="top">
                                    <para>EGES 105-1</para>
                                </entry>
                                <entry valign="top">
                                    <para>DT 408, DT 466 and Internationals 530 Service Manual</para>
                                </entry>
                            </row>
                            <row>
                                <entry valign="top">
                                    <para>1 171 666 R1 and R2</para>
                                </entry>
                                <entry valign="top">
                                    <para>DT 408 Operator's and Maintenance Manual</para>
                                </entry>
                            </row>
                            <row>
                                <entry valign="top">
                                    <para>1 171 667 R1, R2 and R3</para>
                                </entry>
                                <entry valign="top">
                                    <para>DT 466 Operator's and Maintenance Manual</para>
                                </entry>
                            </row>
                            <row>
                                <entry valign="top">
                                    <para>1 171 688 R1, R2 and R3</para>
                                </entry>
                                <entry valign="top">
                                    <para>International&#174; 530 Operator's and Maintenance Manual</para>
                                </entry>
                            </row>
                        </tbody>
                    </tgroup>
                    <caption>Manuals</caption>
                </table>
            </svcsubproc>
        </svcproc>
    </tsi>
</isis>

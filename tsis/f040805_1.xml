        <subjectfile>SYST&#200;ME &#201;LECTRIQUE</subjectfile>
        <subjectgroup>
            <subject>Transmetteur de niveau de carburant de nouvelle conception pour les mod&#232;les HPV</subject>
        </subjectgroup>
        <distribution dealer="yes" fleet="yes" bodybuilder="no" isis_cd="yes"/>

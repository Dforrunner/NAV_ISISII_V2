    <stitle>Spanish - TSI 97-08-03 - Alternadores 21 SI y 30 SI Delco Remy</stitle>
    <tsi id="tsi970803" tsitype="truck" translation="spanish">
        <tsinumber>97-08-03</tsinumber>
        <tsidate>Marzo, 1997</tsidate>
        <subjectfile>EL&#201;CTRICO</subjectfile>
        <subjectgroup>
            <subject>Alternadores 21 SI y 30 SI Delco Remy</subject>
        </subjectgroup>
        <apply>
            <configure>
                <vendorgroup/>

                <vendor>Delco</vendor>

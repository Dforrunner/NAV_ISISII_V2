<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../isisxsl.xsl"?>
<isis lang="en">
    <stitle>TSI 98-12-07 - Radio Frequency Interference Service Kit</stitle>
    <tsi id="tsi981207" tsitype="engine">
        <tsinumber>98-12-07</tsinumber>
        <tsidate>May, 1998</tsidate>
        <subjectfile>ENGINE</subjectfile>
        <subjectgroup>
            <subject>Radio Frequency Interference Service Kit</subject>
        </subjectgroup>
        <apply>
            <configure>
                <engfamgroup/>

                <engfam>DT 466E</engfam>
                <engfam>530E</engfam>
            </configure>
        </apply>
        <description>
            <geninfo>
                <para>A new service kit is available to reduce 2-way and CB radio interference caused by electrical
                    system noise. The kit is available for use on engines equipped with the single box Navistar Engine
                    Control Module (effective with engine serial number 634234).
                </para>
            </geninfo>
        </description>
        <partsinfo>
            <para>The RFI Service Kit Part Number is 1 830 332 C91 and includes:</para>
            <randlist type="bullet">
                <item>
                    <para>Capacitor (.2 microferad)</para>
                </item>
                <item>
                    <para>Screw, M5 x 8 thread forming (2)</para>
                </item>
                <item>
                    <para>Washer</para>
                </item>
                <item>
                    <para>1 171 728 R1 Instruction Sheet</para>
                </item>
            </randlist>
        </partsinfo>
    </tsi>
</isis>

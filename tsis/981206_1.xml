<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../isisxsl.xsl"?>
<isis lang="en">
    <stitle>TSI 98-12-06 - Releasing New Skeleton and Stripped Service Engines for 1998 Model Year</stitle>
    <tsi id="tsi981206" tsitype="engine">
        <tsinumber>98-12-06</tsinumber>
        <tsidate>March, 1998</tsidate>
        <subjectfile>ENGINE</subjectfile>
        <subjectgroup>
            <subject>Releasing New Skeleton and Stripped Service Engines for 1998 Model Year</subject>
        </subjectgroup>
        <apply>
            <configure>
                <engfamgroup/>

                <engfam>T 444E</engfam>
            </configure>
        </apply>
        <description>
            <geninfo>
                <para>To inform the field of new skeleton and stripped service engines for 1998 model year International&#174;
                    T 444E engines. The 1998 model year engines begin with engine serial number 661894.
                </para>
                <specpara>
                    <note>
                        <para>These engines are only for 1998 model year T 444E service.</para>
                    </note>
                </specpara>
            </geninfo>
        </description>
        <partsinfo>
            <table id="t9812061">
                <title>Parts Affected</title>
                <tgroup cols="2">
                    <colspec colname="colspec0" colwidth="2.26in"/>
                    <colspec colname="col1" colwidth="4.38in"/>
                    <thead>
                        <row>
                            <entry colname="colspec0" colsep="1" rowsep="1" valign="top" align="center" charoff="50"
                                   char="">
                                <para>
                                    <emph type="bold">New Part Number</emph>
                                </para>
                            </entry>
                            <entry colname="col1" colsep="1" rowsep="1" valign="top" align="center" charoff="50"
                                   char="">
                                <para>
                                    <emph type="bold">Description</emph>
                                </para>
                            </entry>
                        </row>
                    </thead>
                    <tbody>
                        <row>
                            <entry colname="colspec0" colsep="1" rowsep="1" valign="top" align="center" charoff="50"
                                   char="">
                                <para>1 831 530 C 91</para>
                            </entry>
                            <entry colname="col1">
                                <para>
                                    <emph type="bold">Skeleton Engine for 1998 Model Year</emph>
                                </para>
                                <para>
                                    <emph type="italic">Consists of the following:</emph>
                                </para>
                                <para>Camshaft, bearings and gear</para>
                                <para>Connecting rods and bearings</para>
                                <para>Push rods and cam followers (tappets)</para>
                                <para>Pistons and rings</para>
                                <para>Crankcase, main bearing caps and piston cooling tubes (jets)</para>
                                <para>Crankcase bearings and gear</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="colspec0" align="center">
                                <para>1 831 531 C91</para>
                            </entry>
                            <entry colname="col1">
                                <para>
                                    <emph type="bold">Stripped Service Engine for 1998 Model Year</emph>
                                </para>
                                <para>
                                    <emph type="italic">Consists of the following:</emph>
                                </para>
                                <para>
                                    <emph type="italic">All components found in the Skeleton Engine plus -</emph>
                                </para>
                                <para>Crankcase front and rear covers with gaskets</para>
                                <para>Front and rear oil seals and wear sleeve</para>
                                <para>Vibration damper</para>
                                <para>Cylinder heads with gaskets, valves, levers, and rotators (1 pair)</para>
                                <para>Valve covers with gaskets (1 pair)</para>
                                <para>High pressure pump and reservoir</para>
                                <para>Oil pan and pick-up tube</para>
                                <para>Lubrication oil pump</para>
                                <para>Oil filter</para>
                                <para>Oil cooler with headers</para>
                            </entry>
                        </row>
                    </tbody>
                </tgroup>
            </table>
        </partsinfo>
    </tsi>
</isis>

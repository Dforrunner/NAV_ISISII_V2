<isis lang="en">
    <stitle>TSI 00-12-02 - Model Year 2000 Engine Release</stitle>
    <tsi id="tsi001202" tsitype="engine">
        <tsinumber>00-12-02</tsinumber>
        <tsidate>May, 2000</tsidate>
        <subjectfile>Engines</subjectfile>

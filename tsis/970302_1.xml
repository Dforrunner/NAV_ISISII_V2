    <stitle>TSI 97-03-02 - Hendrickson HAS Air, Rear Suspension Codes 03SAT, 14ULB, 14ULC, 14ULE Vibration Complaints
    </stitle>
    <tsi id="tsi970302" tsitype="truck">
        <tsinumber>97-03-02</tsinumber>
        <tsidate>May, 1997</tsidate>
        <subjectfile>SPRINGS</subjectfile>
        <subjectgroup>
            <subject>Hendrickson HAS Air, Rear Suspension Codes 03SAT, 14ULB, 14ULC, 14ULE Vibration Complaints
            </subject>
        </subjectgroup>
        <apply>
            <configure>
                <vendorgroup/>


<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../isisxsl.xsl"?>
<isis lang="en">
    <stitle>TSI 97-12-04 - Revision to specified Special Torque Values for the DT 466E and International&#174; 530E
        Service Manual, EGES 160-1.
    </stitle>
    <tsi id="tSI971204" tsitype="engine">
        <tsinumber>97-12-04</tsinumber>
        <tsidate>January, 1998</tsidate>
        <subjectfile>Engines</subjectfile>
        <subjectgroup>
            <subject>Revision to specified Special Torque Values for the DT 466E and International&#174; 530E Service
                Manual, EGES 160-1.
            </subject>
        </subjectgroup>
        <apply>
            <configure>
                <engfamgroup/>

                <engfam>DT 466E</engfam>
                <engfam>International&#174; 530E</engfam>
            </configure>
        </apply>
        <description>
            <geninfo>
                <para>The following specified special torque values need to be revised for the EGES 160-1 Service
                    Manual.
                </para>
            </geninfo>
        </description>
        <svcproc svcproctype="replace" graphiccount="0">
            <title>Service Procedure</title>
            <para>Revise the following special torques as listed, refer to
                <hotlnk xrefid="te97120401">Special Torque Values
                    <xref refid="te97120401"/>
                </hotlnk>
                :
            </para>
            <table id="te97120401" frame="all">
                <title>Table 1 Special Torque Values</title>
                <tgroup cols="2" colsep="0" rowsep="0">
                    <colspec colname="col1" colwidth="2.0in"/>
                    <colspec colname="col2" colwidth="1.5in"/>
                    <tbody>
                        <row>
                            <entry>
                                <para>
                                    <emph type="bold">WAS:</emph>
                                </para>
                            </entry>
                            <entry align="right"></entry>
                        </row>
                        <row rowsep="1">
                            <entry>
                                <para>Crankshaft Main Bearing Cap Bolts</para>
                            </entry>
                            <entry align="right">
                                <para>176 Nm (130 lb-ft.)</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>
                                    <emph type="bold">REVISE TO:</emph>
                                </para>
                            </entry>
                            <entry align="right"></entry>
                        </row>
                        <row rowsep="1">
                            <entry>
                                <para>Crankshaft Main Bearing Cap Bolts</para>
                            </entry>
                            <entry align="right">
                                <para>217 Nm (160 lb-ft)</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>
                                    <emph type="bold">WAS:</emph>
                                </para>
                            </entry>
                            <entry align="right"></entry>
                        </row>
                        <row rowsep="1">
                            <entry>
                                <para>Idler Gear Retaining Bolt (upper)</para>
                            </entry>
                            <entry align="right">
                                <para>332 Nm (245 lb-ft)</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>
                                    <emph type="bold">REVISE TO:</emph>
                                </para>
                            </entry>
                            <entry align="right"></entry>
                        </row>
                        <row rowsep="1">
                            <entry>
                                <para>Idler Gear Retaining Bolt (upper)</para>
                            </entry>
                            <entry align="right">
                                <para>325 Nm (240 lb-ft)</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>
                                    <emph type="bold">WAS:</emph>
                                </para>
                            </entry>
                            <entry align="right"></entry>
                        </row>
                        <row rowsep="1">

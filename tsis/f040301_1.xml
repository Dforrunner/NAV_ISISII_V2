                            <row>
                                <entry>
                                    <para>
                                        <emph type="bold">Code HA</emph>
                                    </para>
                                </entry>
                                <entry>
                                    <para>
                                        <emph type="bold">Hauteur d'assiette (r&#233;f. seul.)</emph>
                                    </para>
                                </entry>
                                <entry>
                                    <para>

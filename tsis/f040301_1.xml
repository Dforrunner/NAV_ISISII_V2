<isis lang="fr">
    <stitle>French - TSI 04-03-01 - Caract&#233;ristiques de r&#233;glage des dimensions de la hauteur des ressorts
        pneumatiques de la suspension pneumatique et des dimensions de course d'essieu
    </stitle>
    <tsi id="tSI040301" tsitype="truck" translation="french">
        <tsinumber>04-03-01</tsinumber>
        <replace>
            <tsinumber>03-03-01R</tsinumber>
        </replace>
        <tsidate>April, 2004</tsidate>
        <subjectfile>RESSORTS</subjectfile>
        <subjectgroup>
            <subject>Caract&#233;ristiques de r&#233;glage des dimensions de la hauteur des ressorts pneumatiques de la
                suspension pneumatique et des dimensions de course d'essieu
            </subject>
        </subjectgroup>
        <apply>
            <configure>
                <vendorgroup/>

                <vendor>International</vendor>
            </configure>
            <configure>
                <vendorgroup/>

                <vendor>Hendrickson</vendor>
            </configure>
            <configure>
                <vendorgroup/>

                <vendor>Neway</vendor>

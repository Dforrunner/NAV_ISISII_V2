    <stitle>TSI 97-01-01 - Stamped Steel "Dog Bone" Crossmember Identification</stitle>
    <tsi id="tsi970101" tsitype="truck">
        <tsinumber>97-01-01</tsinumber>
        <tsidate>September, 1997</tsidate>

<isis lang="en">
    <stitle>TSI 04-07-01 - New Design Turbocharger Flanged Exhaust Pipe on 8600 Models, Build Date Starting August 9,
        2004, with Cummins ISM Engine
    </stitle>
    <tsi id="tsi040701" tsitype="truck">
        <tsinumber>04-07-01</tsinumber>
        <tsidate>November, 2004</tsidate>
        <subjectfile>EXHAUST SYSTEM</subjectfile>
        <subjectgroup>
            <subject>New Design Turbocharger Flanged Exhaust Pipe on 8600 Models, Build Date Starting August 9, 2004,
                with Cummins ISM Engine
            </subject>
        </subjectgroup>
        <distribution dealer="yes" fleet="yes" bodybuilder="yes" isis_cd="yes"/>
        <apply>
            <configure>
                <truckmodelgroup/>

                <truckmodel>8600
                    <daterange>
                        <startdate>08/09/2004</startdate>
                    </daterange>
                </truckmodel>
            </configure>
        </apply>
        <description>
            <geninfo>
                <para>A new design turbocharger flanged exhaust pipe has been released for use with Cummins ISM engines
                    in 8600 Models. The new pipe no longer requires an external support bracket.
                </para>
            </geninfo>
        </description>

        <svcproc svcproctype="replace" graphiccount="2">
            <title>Service Procedure</title>
            <specpara>
                <warning>
                    <para>To avoid personal injury or death, shift transmission to park or neutral, set parking brake,
                        and block wheels before doing diagnostic or service procedures.
                    </para>
                </warning>
            </specpara>
            <figure figsize="pg-wide">
                <graphic filename="../afcfigures/g0790801.webp" type="webp" scalefit="1" scalefitwidth="248px"
                         scalefitheight="272px"/>
                <caption>Figure 1.</caption>
            </figure>
            <step1>
                <para count="1">Clean hood, (DRIVERS SIDE) below black intake grille area, of grease and dirt prior to
                    applying the label (Figure 1).
                </para>
            </step1>
            <step1>
                <para count="1">Affix label to hood as per Figure 2.</para>
            </step1>
            <figure figsize="pg-wide">
                <graphic filename="../afcfigures/g0790802.webp" type="webp" scalefit="1" scalefitwidth="248px"
                         scalefitheight="272px"/>
                <caption>Figure 2.</caption>
            </figure>
        </svcproc>
        <laborinfo>
            <para>Operation number must appear on all claims.</para>
            <table id="t0790802" tblwidth="pg-wide">
                <title>Labor Information</title>
                <tgroup cols="3" align="center">
                    <colspec colname="col1" colwidth="1.53in"/>

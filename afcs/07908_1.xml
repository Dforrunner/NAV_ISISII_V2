    <stitle>AFC G-07908 - Warning Labels</stitle>
    <afc id="afc07908">
        <afcnum>G-07908</afcnum>
        <afcdate>July, 2007</afcdate>
        <subjectfile>Front End Sheet Metal</subjectfile>
        <subject>Warning Labels</subject>
        <apply>
            <configure>
                <truckmodelgroup/>

                <truckmodel>7300
                    <daterange>
                        <startdate>05/31/2006</startdate>
                        <enddate>05/31/2007</enddate>
                    </daterange>
                </truckmodel>

<isis lang="en">
    <stitle>AFC 96906 - Installation of Aeroquip Quick Connect (STC) Fitting Locking Clips</stitle>
    <afc id="AFC96906">
        <afcnum>96906</afcnum>
        <afcdate>November, 1996</afcdate>
        <subjectfile>TRANSMISSION</subjectfile>
        <subject>Installation of Aeroquip Quick Connect (STC) Fitting Locking Clips</subject>
        <apply>
            <configure>
                <vendorgroup/>

                <vendor>Aeroquip</vendor>
                <truckmodelgroup/>

                <truckmodel>2000
                    <daterange>
                        <startdate>05/01/1995</startdate>
                        <enddate>09/20/1996</enddate>
                    </daterange>
                </truckmodel>
                <truckmodel>5000
                    <daterange>
                        <startdate>05/01/1995</startdate>
                        <enddate>09/20/1996</enddate>
                    </daterange>
                </truckmodel>

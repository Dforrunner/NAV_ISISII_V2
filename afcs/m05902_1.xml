            <table id="t0590201">
                <tgroup cols="3" align="center">
                    <colspec colname="col1" colwidth="0.47*"/>
                    <colspec colname="col2" colwidth="2.11*"/>
                    <colspec colname="col3" colwidth="0.38*"/>
                    <thead>
                        <row>
                            <entry>
                                <para>
                                    <emph type="bold">N&#250;mero de pieza</emph>
                                </para>
                            </entry>
                            <entry>
                                <para>
                                    <emph type="bold">Descripci&#243;n</emph>
                                </para>
                            </entry>
                            <entry>
                                <para>
                                    <emph type="bold">Cantidad</emph>
                                </para>
                            </entry>

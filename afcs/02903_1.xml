<isis lang="en">
    <stitle>AFC 02903 - New Windshield Washer Solvent Reservoir for 73/74/7500 and 8500 Models Built Between May 28,
        2001 and February 27, 2002
    </stitle>
    <afc id="afc02903">
        <afcnum>02903</afcnum>
        <afcdate>July, 2002</afcdate>
        <subjectfile>CAB</subjectfile>
        <subject>New Windshield Washer Solvent Reservoir for 73/74/7500 and 8500 Models Built Between May 28, 2001 and
            February 27, 2002
        </subject>
        <apply>
            <configure>
                <truckmodelgroup/>

                <truckmodel>7300
                    <daterange>
                        <startdate>05/28/2001</startdate>
                        <enddate>02/27/2002</enddate>
                    </daterange>
                </truckmodel>
                <truckmodel>7400
                    <daterange>
                        <startdate>05/28/2001</startdate>
                        <enddate>02/27/2002</enddate>
                    </daterange>
                </truckmodel>
                <truckmodel>7500
                    <daterange>
                        <startdate>05/28/2001</startdate>
                        <enddate>02/27/2002</enddate>
                    </daterange>

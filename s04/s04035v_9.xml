<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../isisxsl.xsl"?>
<isis lang="en">
    <stitle>ANTILOCK BRAKES/TRACTION CONTROL: BENDIX - TROUBLESHOOTING - 9800 MODEL - INSPECTION FOR ILLUMINATED LEDS,
        PART A
    </stitle>
    <svcsection/>
    <svcproc svcproctype="IDcomponent" graphiccount="0">
        <title>INSPECTION FOR ILLUMINATED LEDS, PART A</title>
        <table id="t0403503" tblwidth="full-page" frame="all" numbered="no" view="reveal">
            <title>Inspection For Illuminated LEDs, Part A</title>
            <tgroup cols="5" align="center" charoff="50" char="c">
                <colspec colname="1" colwidth="1.10in"/>
                <colspec colname="2" colwidth="1.10in"/>
                <colspec colname="3" colwidth="1.10in"/>
                <colspec colname="4" colwidth="1.10in"/>
                <colspec colname="5" colwidth="1.10in"/>
                <spanspec namest="1" nameend="5" spanname="s1t5" align="center"/>
                <thead>
                    <row>
                        <entry colspan="5" spanname="s1t5">
                            <para>Perform
                                <hotlnk document="s04035v_7.xml" xrefid="t0403501">INITIAL START-UP TEST PROCEDURE
                                    <xref refid="t0403501"/>
                                </hotlnk>
                                , before performing this test.
                            </para>
                        </entry>
                    </row>
                </thead>
                <tbody valign="top">
                    <row>
                        <entry colname="1">
                            <para>STEP</para>
                        </entry>
                        <entry colname="2">
                            <para>KEY</para>
                        </entry>
                        <entry colname="3">
                            <para>ACTION</para>
                        </entry>
                        <entry colname="4">
                            <para>YES / IN SPEC.</para>
                        </entry>
                        <entry colname="5">
                            <para>NO / OUT OF SPEC.</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="1">
                            <para>1.</para>
                        </entry>
                        <entry colname="2">
                            <para>On</para>
                        </entry>
                        <entry colname="3">
                            <para>Is GREEN VLT LED illuminated?</para>
                        </entry>
                        <entry colname="4">
                            <para>Go to next step.</para>
                        </entry>
                        <entry colname="5">
                            <para>Go to Step 4.</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="1">
                            <para>2.</para>
                        </entry>
                        <entry colname="2">
                            <para>On</para>
                        </entry>
                        <entry colname="3">
                            <para>Is GREEN VLT LED flashing?</para>
                        </entry>
                        <entry colname="4">
                            <para>Go to
                                <hotlnk document="s04035v_11.xml" xrefid="t0403505">TESTING FOR POWER TO THE CONTROLLER
                                    <xref refid="t0403505"/>.
                                </hotlnk>
                            </para>
                        </entry>
                        <entry colname="5">
                            <para>Go to next step.</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="1">
                            <para>3.</para>
                        </entry>
                        <entry colname="2">
                            <para>On</para>
                        </entry>
                        <entry colname="3">
                            <para>Note if any RED LEDS are illuminated.</para>
                        </entry>
                        <entry colname="4">
                            <para>Record which LEDS are ON, then go to
                                <hotlnk document="s04035v_10.xml" xrefid="t0403504">INSPECTION FOR ILLUMINATED LEDS,
                                    PART B
                                    <xref refid="t0403504"/>
                                </hotlnk>
                                .
                            </para>
                        </entry>
                        <entry colname="5">
                            <para>Go to
                                <hotlnk document="s04035v_23.xml" xrefid="t0403517">TESTING FOR FALSE INDICATION CAUSED
                                    BY ABS WARNING LIGHT
                                    <xref refid="t0403517"/>
                                </hotlnk>
                                . Also, go to
                                <hotlnk document="s04035v_24.xml" xrefid="t0403518">TESTING FOR FALSE INDICATION CAUSED
                                    BY WHEEL SPEED COMPONENTS
                                    <xref refid="t0403518"/>
                                </hotlnk>
                                .
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="1">
                            <para>4.</para>
                        </entry>
                        <entry colname="2">
                            <para>On</para>
                        </entry>
                        <entry colname="3">
                            <para>Note if any RED LEDS are illuminated.</para>
                        </entry>
                        <entry colname="4">
                            <para>Replace the controller.</para>
                        </entry>
                        <entry colname="5">
                            <para>Go to
                                <hotlnk document="s04035v_11.xml" xrefid="t0403505">TESTING FOR POWER TO THE CONTROLLER
                                    <xref refid="t0403505"/>
                                </hotlnk>
                                .
                            </para>
                        </entry>
                    </row>
                </tbody>
            </tgroup>
        </table>
    </svcproc>
    <table>
        <tgroup cols="3">
            <tbody>
                <row>
                    <entry>
                        <para>
                            <hotlnk document="s04035v_8.xml">
                                <con_previous/>
                            </hotlnk>
                        </para>
                    </entry>
                    <entry>
                        <para>
                            <hotlnk document="s04035v_10.xml">
                                <con_next/>
                            </hotlnk>
                        </para>
                    </entry>
                    <entry>
                        <para>
                            <hotlnk document="s04035v.htm" target="_top">
                                <con_toc/>
                            </hotlnk>
                        </para>
                    </entry>
                </row>
            </tbody>
        </tgroup>
    </table>
</isis>

    <stitle>ELECTRICAL SYSTEM TROUBLESHOOTING GUIDE - 9800 MODEL - LIGHT SYSTEMS - TROUBLESHOOTING - Cab Dome, Reading,
        Courtesy
    </stitle>
    <svcman/>
    <svcproc svcproctype="troubleshoot" graphiccount="0">
        <title>Cab Dome, Reading, Courtesy</title>
        <table id="t0828004" tblwidth="full-page" frame="all" numbered="no" view="reveal">
            <title>Cab Dome, Reading, Courtesy</title>
            <tgroup cols="7" colsep="1" rowsep="1" align="center" charoff="50">
                <colspec colname="1" charoff="50" colwidth="0.59in"/>
                <colspec colname="2" charoff="50" colwidth="0.55in"/>

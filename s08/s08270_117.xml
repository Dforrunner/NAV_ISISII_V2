            </specpara>
            <table id="t0827621" tblwidth="full-page" frame="all" numbered="no" view="reveal">
                <title>Testing Cruise Set/Resume Switch and Circuits - Cummins</title>
                <tgroup cols="7" colsep="1" rowsep="1" align="center" charoff="50">
                    <colspec colname="1" charoff="50" colwidth="0.59in"/>
                    <colspec colname="2" charoff="50" colwidth="0.50in"/>
                    <colspec colname="3" charoff="50" colwidth="1.74in"/>
                    <colspec colname="4" charoff="50" colwidth="0.94in"/>
                    <colspec colname="5" charoff="50" colwidth="0.69in"/>
                    <colspec colname="6" charoff="50" colwidth="0.75in"/>
                    <colspec colname="7" charoff="50" colwidth="1.44in"/>
                    <thead>
                        <row>
                            <entry colname="1" rotate="0">
                                <para>
                                    <emph type="bold">STEP</emph>
                                </para>
                            </entry>
                            <entry colname="2" rotate="0">
                                <para>
                                    <emph type="bold">KEY</emph>
                                </para>

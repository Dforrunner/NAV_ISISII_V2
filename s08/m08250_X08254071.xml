                    <colspec colname="4" charoff="50" colwidth="0.81in"/>
                    <colspec colname="5" charoff="50" colwidth="0.95in"/>
                    <colspec colname="6" charoff="50" colwidth="0.93in"/>
                    <colspec colname="7" charoff="50" colwidth="1.42in"/>
                    <spanspec namest="2" nameend="7" spanname="SPAN1"/>
                    <thead>
                        <row>
                            <entry colname="1" rotate="0">
                                <para>
                                    <emph type="bold">PASO</emph>
                                </para>
                            </entry>
                            <entry colname="2" rotate="0">
                                <para>
                                    <emph type="bold">LLAVE IGNI.</emph>
                                </para>
                            </entry>
                            <entry colname="3" rotate="0">
                                <para>
                                    <emph type="bold">ACCI&#211;N</emph>
                                </para>
                            </entry>
                            <entry colname="4" rotate="0">

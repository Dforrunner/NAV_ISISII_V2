<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../isisxsl.xsl"?>
<isis lang="en">
    <stitle>ELECTRICAL SYSTEM TROUBLESHOOTING GUIDE / 2000 4000 and 8000 MODELS BUILT OCTOBER 1, 1998 AND AFTER -
        SPEEDOMETER/TACHOMETER, GAUGES AND WARNING LIGHTS - 9800 - Troubleshooting - VOLTMETER GAUGE, CIRCUITS AND
        CLUSTER
    </stitle>
    <svcsection/>
    <svcproc svcproctype="troubleshoot" graphiccount="0">
        <title>VOLTMETER GAUGE, CIRCUITS AND CLUSTER</title>
        <svcprocinfo>
            <para>Refer to
                <hotlnk document="s08245x_8.xml" xrefid="x0824508">Voltmeter
                    <xref refid="x0824508"/>
                </hotlnk>
                for operation description of the voltmeter.
            </para>
        </svcprocinfo>
        <svcsubproc>
            <title>Voltmeter Gauge and Circuit Quick Test</title>
            <para>This test will determine if the gauge and circuits are working</para>
            <table id="t0824508">
                <title>Voltmeter Gauge and Circuit Quick Test</title>
                <tgroup cols="7" colsep="0" rowsep="0">
                    <colspec colname="1" colwidth="0.58in"/>
                    <colspec colname="2" colwidth="0.58in"/>
                    <colspec colname="3" colwidth="1.54in"/>
                    <colspec colname="4" colwidth="0.96in"/>
                    <colspec colname="5" colwidth="0.71in"/>
                    <colspec colname="6" colwidth="0.92in"/>
                    <colspec colname="7" colwidth="1.23in"/>
                    <thead>
                        <row>
                            <entry>
                                <para>
                                    <emph type="bold">STEP</emph>
                                </para>
                            </entry>
                            <entry>
                                <para>
                                    <emph type="bold">KEY</emph>
                                </para>
                            </entry>
                            <entry>
                                <para>
                                    <emph type="bold">ACTION</emph>
                                </para>
                            </entry>
                            <entry>
                                <para>
                                    <emph type="bold">TEST POINTS</emph>
                                </para>
                            </entry>
                            <entry>
                                <para>
                                    <emph type="bold">SPEC.</emph>
                                </para>
                            </entry>
                            <entry>
                                <para>
                                    <emph type="bold">YES- IN SPEC.</emph>
                                </para>
                            </entry>
                            <entry>
                                <para>
                                    <emph type="bold">NO-OUT OF SPEC.</emph>
                                </para>
                            </entry>
                        </row>
                    </thead>
                    <tbody valign="top">
                        <row>
                            <entry>
                                <para>1.</para>
                            </entry>
                            <entry>
                                <para>Off</para>
                            </entry>
                            <entry>
                                <para>Measure voltage at batteries.</para>
                            </entry>
                            <entry>
                                <para>Positive and negative battery terminals.</para>
                            </entry>
                            <entry>
                                <para></para>
                            </entry>
                            <entry>
                                <para>Go to next step.</para>
                            </entry>
                            <entry>
                                <para>Go to next step.</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>2.</para>
                            </entry>
                            <entry colname="2">
                                <para>On</para>
                            </entry>
                            <entry colname="3">
                                <para>Note gauge reading in cluster.</para>
                            </entry>
                            <entry colname="4">
                                <para>Gauge reading.</para>
                            </entry>
                            <entry colname="5">
                                <para>Voltage at battery &#177; 0.5 volt.</para>
                            </entry>
                            <entry colname="6">
                                <para>Gauge and circuits check good.</para>
                            </entry>
                            <entry colname="7">
                                <para>Go to next step.</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>3.</para>
                            </entry>
                            <entry>
                                <para>On</para>
                            </entry>
                            <entry>
                                <para>Remove gauge. Measure voltage across positive and negative voltmeter pins
                                    <hotlnk document="s08245x_27.xml" xrefid="f0824518">(Figure 18)</hotlnk>.
                                </para>
                            </entry>
                            <entry>
                                <para>Across positive and negative pins.</para>
                            </entry>
                            <entry>
                                <para>Voltage at battery &#177; 0.5 volt.</para>
                            </entry>
                            <entry>
                                <para>Replace gauge.</para>
                            </entry>
                            <entry>
                                <para>Circuit possibly defective. Go to Voltmeter Circuit and Cluster Test.</para>
                            </entry>
                        </row>
                    </tbody>
                </tgroup>
            </table>
        </svcsubproc>
        <svcsubproc>
            <title>Voltmeter Circuit and Cluster Test</title>
            <specpara>
                <note>
                    <para>Perform Voltmeter Gauge and Circuit Quick Test prior to performing this test.</para>
                </note>
            </specpara>
            <table id="t0824509">
                <title>Voltmeter Circuit and Cluster Test</title>
                <tgroup cols="7" colsep="0" rowsep="0">
                    <colspec colname="1" colwidth="0.58in"/>
                    <colspec colname="2" colwidth="0.58in"/>
                    <colspec colname="3" colwidth="1.54in"/>
                    <colspec colname="4" colwidth="0.96in"/>
                    <colspec colname="5" colwidth="0.71in"/>
                    <colspec colname="6" colwidth="0.92in"/>
                    <colspec colname="7" colwidth="1.24in"/>
                    <thead>
                        <row>
                            <entry>
                                <para>
                                    <emph type="bold">STEP</emph>
                                </para>
                            </entry>
                            <entry>
                                <para>
                                    <emph type="bold">KEY</emph>
                                </para>
                            </entry>
                            <entry>
                                <para>
                                    <emph type="bold">ACTION</emph>
                                </para>
                            </entry>
                            <entry>
                                <para>
                                    <emph type="bold">TEST POINTS</emph>
                                </para>
                            </entry>
                            <entry>
                                <para>
                                    <emph type="bold">SPEC.</emph>
                                </para>
                            </entry>
                            <entry>
                                <para>
                                    <emph type="bold">YES- IN SPEC.</emph>
                                </para>
                            </entry>
                            <entry>
                                <para>
                                    <emph type="bold">NO-OUT OF SPEC.</emph>
                                </para>
                            </entry>
                        </row>
                    </thead>
                    <tbody valign="top">
                        <row>
                            <entry>
                                <para>1.</para>
                            </entry>
                            <entry>
                                <para>Off</para>
                            </entry>
                            <entry>
                                <para>Remove and test 10A fuse F4-A for open condition.</para>
                            </entry>
                            <entry>
                                <para>F4-A</para>
                            </entry>
                            <entry>
                                <para>&#60;1 ohm.</para>
                            </entry>
                            <entry>
                                <para>(Do not replace fuse yet). Go to next step.</para>
                            </entry>
                            <entry>
                                <para>Locate cause of overload condition, then replace fuse.</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>2.</para>
                            </entry>
                            <entry colname="2">
                                <para>On</para>
                            </entry>
                            <entry colname="3">
                                <para>At fuse panel F4-A, measure voltage to ground at circuit 13B.</para>
                            </entry>
                            <entry colname="4">
                                <para>F4-A, 13B to ground.</para>
                            </entry>
                            <entry colname="5">
                                <para>12&#177;1.5 volts.</para>
                            </entry>
                            <entry colname="6">
                                <para>Replace fuse. Go to next step.</para>
                            </entry>
                            <entry colname="7">
                                <para>Locate cause of low or no voltage in circuit 13B/13 between fuse F4-A and key
                                    switch, repair, then replace fuse.
                                </para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>3.</para>
                            </entry>
                            <entry colname="2">
                                <para>On</para>
                            </entry>
                            <entry colname="3">
                                <para>Remove connector (73) from gauge cluster. At connector (73) measure voltage to
                                    ground at circuit 27.
                                </para>
                            </entry>
                            <entry colname="4">
                                <para>(73), 27 to ground.</para>
                            </entry>
                            <entry colname="5">
                                <para>12&#177;1.5 volts.</para>
                            </entry>
                            <entry colname="6">
                                <para>Go to next step.</para>
                            </entry>
                            <entry colname="7">
                                <para>Locate cause of low or no voltage in circuit 27/13D, then repair.</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>4.</para>
                            </entry>
                            <entry colname="2">
                                <para>Off</para>
                            </entry>
                            <entry colname="3">
                                <para>At connector (73) measure resistance to ground at circuit 11-GV.</para>
                            </entry>
                            <entry colname="4">
                                <para>(73), 11-GV to ground.</para>
                            </entry>
                            <entry colname="5">
                                <para>&#60;1 ohm.</para>
                            </entry>
                            <entry colname="6">
                                <para>Go to next step.</para>
                            </entry>
                            <entry colname="7">
                                <para>Locate open or poor connection in circuit 11-GV/11-GF, then repair.</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>5.</para>
                            </entry>
                            <entry colname="2">
                                <para>Off</para>
                            </entry>
                            <entry colname="3">
                                <para>Measure resistance between cluster connector (73) cavity 2 and voltmeter negative
                                    pin
                                    <hotlnk document="s08245x_27.xml" xrefid="f0824518">(Figure 18)</hotlnk>.
                                </para>
                            </entry>
                            <entry colname="4">
                                <para>(73), cavity 2 and negative pin.</para>
                            </entry>
                            <entry colname="5">
                                <para>&#60;1 ohm.</para>
                            </entry>
                            <entry colname="6">
                                <para>Go to next step.</para>
                            </entry>
                            <entry colname="7">
                                <para>Replace the cluster.</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>6.</para>
                            </entry>
                            <entry colname="2">
                                <para>Off</para>
                            </entry>
                            <entry colname="3">
                                <para>Measure resistance between cluster connector (73) cavity 7 and voltmeter positive
                                    pin
                                    <hotlnk document="s08245x_27.xml" xrefid="f0824518">(Figure 18)</hotlnk>
                                </para>
                            </entry>
                            <entry colname="4">
                                <para>(73), cavity 7 and positive pin.</para>
                            </entry>
                            <entry colname="5">
                                <para>&#60;1 ohm.</para>
                            </entry>
                            <entry colname="6">
                                <para>Circuits and cluster test good. Replace gauge and reconnect (73).</para>
                            </entry>
                            <entry colname="7">
                                <para>Replace the cluster.</para>
                            </entry>
                        </row>
                    </tbody>
                </tgroup>
            </table>
        </svcsubproc>
    </svcproc>

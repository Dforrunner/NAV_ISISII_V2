        <svcproc svcproctype="describe" graphiccount="1">
            <title>FUNCIONES DEL CIRCUITO</title>
            <para>Vea el
                <hotlnk xrefid="f0825860">diagrama de funci&#243;n del reloj</hotlnk>.
            </para>
            <figure id="f0825860" figsize="pg-wide">

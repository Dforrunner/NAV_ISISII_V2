    <svcproc svcproctype="troubleshoot" graphiccount="0">
        <title>Auxiliary Radio Controls W/PRO SLEEPER</title>
        <table id="t0806202" tblwidth="full-page" frame="all" numbered="no" view="reveal">
            <title>Auxiliary Radio Controls With PRO SLEEPER</title>
            <tgroup cols="7" align="center" charoff="50" char="c">
                <colspec colname="1" colwidth="0.65in"/>
                <colspec colname="2" colwidth="0.67in"/>

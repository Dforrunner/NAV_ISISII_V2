    <stitle>ELECTRICAL SYSTEM TROUBLESHOOTING GUIDE - 5000, 9100, 9200, 9400, AND 9900 MODELS - CUMMINS ISM AND AHD
        ENGINE SYSTEM
    </stitle>
    <svcman/>
    <svcsection id="s08200" svcsectype="prodspec" division="truck" formnum="CTS-5301-9" date="02/10/1999">
        <title>CUMMINS ISM AND AHD ENGINE SYSTEM</title>
        <apply>
            <configure>
                <truckmodelgroup/>

                <truckmodel>5000
                    <daterange>
                        <startdate>10/01/1998</startdate>
                    </daterange>

                    <tbody>
                        <row rowsep="0">
                            <entry colname="1" align="center">
                                <para>SYSTEM VOLTAGE</para>
                            </entry>
                            <entry colname="2" align="center">
                                <para>RATED VOLTAGE</para>
                            </entry>
                            <entry colname="3" align="center">
                                <para>OPERATING RANGE (Voltage)</para>
                            </entry>
                        </row>
                        <row rowsep="0">

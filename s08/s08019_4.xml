                                <para>12</para>
                            </entry>
                            <entry colname="2" align="center">
                                <para>14.2</para>
                            </entry>
                            <entry colname="3" align="center">
                                <para>13.5 - 14.5</para>
                            </entry>
                        </row>
                    </tbody>
                </tgroup>
            </table>
        </svcsubproc>

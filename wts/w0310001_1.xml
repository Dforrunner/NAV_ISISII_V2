<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../isisxsl.xsl"?>
<isis lang="en">
    <stitle>Procedural Guideline - FRONT SPRING FRONT MOUNTING BRACKET (FRONT), REPLACE</stitle>
    <wtsproc id="w0310001" wtscode="A03-140" group="03">
        <wtsdescrip>FRONT SPRING FRONT MOUNTING BRACKET (FRONT), REPLACE</wtsdescrip>
        <model>1000</model>
        <modelyr>1996</modelyr>
        <engine>T 444E</engine>
        <tran>International 6+1</tran>
        <config>RPS Model</config>
        <svcproc svcproctype="remove" graphiccount="0">
            <title>REMOVE</title>
            <step1>
                <para count="1">Chock wheels.</para>
            </step1>
            <step1>
                <para count="1">Disconnect hood support shocks and raise hood support for access.</para>
            </step1>
            <step1>
                <para count="1">Raise vehicle support with frame stands.</para>
            </step1>
            <step1>
                <para count="1">Remove front eye bolt.</para>
            </step1>
            <step1>
                <para count="1">Use bottle jack to support radiator crossmember.</para>
            </step1>
            <step1>
                <para count="1">Remove bolts retaining front hanger and remove hanger torque bolts.</para>
            </step1>
        </svcproc>
        <svcproc svcproctype="install" graphiccount="0">
            <title>INSTALL</title>
            <step1>
                <para count="1">Install hanger and retaining bolts. Torque bolts.</para>
            </step1>
            <step1>
                <para count="1">Remove bottle jack.</para>
            </step1>
            <step1>
                <para count="1">Install eye bolt and torque nut.</para>
            </step1>
            <step1>
                <para count="1">Remove frame stands.</para>
            </step1>
            <step1>
                <para count="1">Connect hood support shocks.</para>
            </step1>
            <step1>
                <para count="1">Remove wheel chocks.</para>
            </step1>
        </svcproc>
    </wtsproc>
</isis>

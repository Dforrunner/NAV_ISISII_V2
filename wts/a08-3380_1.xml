<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../isisxsl.xsl"?>
<isis lang="en">
    <stitle>Procedural Guideline - FUEL GAUGE SENDING UNIT; REPLACE</stitle>
    <wtsproc id="a08-3380" wtscode="A08-3380" group="08">
        <wtsdescrip>FUEL GAUGE SENDING UNIT; REPLACE</wtsdescrip>
        <model>4200</model>
        <model>4300</model>
        <model>4400</model>
        <modelyr>2001</modelyr>
        <engine>Any</engine>
        <tran>Any</tran>
        <config></config>
        <svcproc svcproctype="remove" graphiccount="0">
            <title>REMOVE</title>
            <step1>
                <para count="1">Install wheel chocks.</para>
            </step1>
            <step1>
                <para count="1">Remove diesel fuel from tank using transfer pump if applicable.</para>
            </step1>
            <step1>
                <para count="1">Remove fuel tank step with mount brackets attached.</para>
            </step1>
            <step1>
                <para count="1">Disconnect fuel tank straps.</para>
            </step1>
            <step1>
                <para count="1">Disconnect fuel lines from tank, include disconnect fuel sender wiring.</para>
            </step1>
            <step1>
                <para count="1">Remove fuel tank.</para>
            </step1>
            <step1>
                <para count="1">Remove fuel sender from tank.</para>
            </step1>
        </svcproc>
        <svcproc svcproctype="install" graphiccount="0">
            <title>INSTALL</title>
            <step1>
                <para count="1">Install fuel sender to tank, include gasket.</para>
            </step1>
            <step1>
                <para count="1">Install fuel tank.</para>
            </step1>
            <step1>
                <para count="1">Connect fuel lines to tank, include connect fuel sender wiring.</para>
            </step1>
            <step1>
                <para count="1">Connect fuel tank strap, include install rubber protection straps.</para>
            </step1>
            <step1>
                <para count="1">Install fuel tank step with mount brackets attached.</para>
            </step1>
            <step1>
                <para count="1">Install diesel fuel into tank using transfer pump if applicable.</para>
            </step1>
            <step1>
                <para count="1">Check fuel gauge operation.</para>
            </step1>
            <step1>
                <para count="1">Remove wheel chocks.</para>
            </step1>
        </svcproc>
    </wtsproc>
</isis>

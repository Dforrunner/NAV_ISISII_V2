<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../isisxsl.xsl"?>
<isis lang="en">
    <stitle>Procedural Guideline - FRONT SPRING REAR MOUNTING BRACKET (REAR), REPLACE</stitle>
    <wtsproc id="w0310002" wtscode="A03-150" group="03">
        <wtsdescrip>FRONT SPRING REAR MOUNTING BRACKET (REAR), REPLACE</wtsdescrip>
        <model>1000</model>
        <modelyr>1996</modelyr>
        <engine>T 444E</engine>
        <tran>International 6+1</tran>
        <config>RPS Model</config>
        <svcproc svcproctype="remove" graphiccount="0">
            <title>REMOVE</title>
            <step1>
                <para count="1">Chock wheels.</para>
            </step1>
            <step1>
                <para count="1">Raise vehicle support with frame stands.</para>
            </step1>
            <step1>
                <para count="1">Support axle with floor jack.</para>
            </step1>
            <step1>
                <para count="1">Remove rear eye bolt.</para>
            </step1>
            <step1>
                <para count="1">Lower axle.</para>
            </step1>
            <step1>
                <para count="1">Remove bolts retaining rear hanger and remove hanger; include raise axle.</para>
            </step1>
            <step1>
                <para count="1">Remove shackle from hanger.</para>
            </step1>
        </svcproc>
        <svcproc svcproctype="install" graphiccount="0">
            <title>INSTALL</title>
            <step1>
                <para count="1">Install hanger shackle.</para>
            </step1>
            <step1>
                <para count="1">Install hanger and raise/lower axle. Torque bolts.</para>
            </step1>
            <step1>
                <para count="1">Install rear eye bolt and torque nut.</para>
            </step1>
            <step1>
                <para count="1">Remove frame stands and lower vehicle.</para>
            </step1>
            <step1>
                <para count="1">Remove wheel chocks.</para>
            </step1>
        </svcproc>
    </wtsproc>
</isis>

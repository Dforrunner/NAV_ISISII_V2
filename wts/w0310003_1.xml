<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../isisxsl.xsl"?>
<isis lang="en">
    <stitle>Procedural Guideline - FRONT SPRING SHACKLE AND/OR BUSHING, REPLACE</stitle>
    <wtsproc id="w0310003" wtscode="A03-190" group="03">
        <wtsdescrip>FRONT SPRING SHACKLE AND/OR BUSHING, REPLACE</wtsdescrip>
        <model>1000</model>
        <modelyr>1996</modelyr>
        <engine>T 444E</engine>
        <tran>International 6+1</tran>
        <config>RPS Model</config>
        <svcproc svcproctype="remove" graphiccount="0">
            <title>REMOVE</title>
            <step1>
                <para count="1">Chock wheels.</para>
            </step1>
            <step1>
                <para count="1">Raise vehicle support with frame stands.</para>
            </step1>
            <step1>
                <para count="1">Support axle with floor jack.</para>
            </step1>
            <step1>
                <para count="1">Remove spring-shackle eye bolt.</para>
            </step1>
            <step1>
                <para count="1">Remove shackle/hanger eye bolt.</para>
            </step1>
            <step1>
                <para count="1">Remove shackle.</para>
            </step1>
        </svcproc>
        <svcproc svcproctype="install" graphiccount="0">
            <title>INSTALL</title>
            <step1>
                <para count="1">Install shackle.</para>
            </step1>
            <step1>
                <para count="1">Install shackle/hanger eye bolt and torque nut.</para>
            </step1>
            <step1>
                <para count="1">Install spring-shackle eye bolt and torque bolt.</para>
            </step1>
            <step1>
                <para count="1">Remove frame stands and lower vehicle.</para>
            </step1>
            <step1>
                <para count="1">Remove wheel chocks.</para>
            </step1>
        </svcproc>
    </wtsproc>
</isis>

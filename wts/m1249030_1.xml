    <stitle>Spanish - Procedural Guideline - COLECTOR DE ACEITE Y/O JUNTA (I-6), REEMPLAZO</stitle>
    <wtsproc id="w1249030" translation="spanish" wtscode="A12-1220" group="12">
        <wtsdescrip>COLECTOR DE ACEITE Y/O JUNTA (I-6), REEMPLAZO</wtsdescrip>
        <model>4900</model>
        <model>3800</model>
        <model>3600</model>
        <model>3000</model>
        <modelyr>1995</modelyr>
        <engine>DT 466</engine>

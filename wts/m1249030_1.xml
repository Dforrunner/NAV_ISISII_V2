        <config></config>
        <svcproc svcproctype="remove" graphiccount="0">
            <title>RETIRO</title>
            <step1>
                <para count="1">Drene el aceite de la caja del cig&#252;e&#241;al.</para>
            </step1>
            <step1>
                <para count="1">Quite los pernos y espaciadores del colector de aceite.</para>
            </step1>
            <step1>
                <para count="1">Quite el colector de aceite.</para>
            </step1>
            <step1>
                <para count="1">Limpie el colector de aceite y las superficies de la junta.</para>
            </step1>
        </svcproc>
        <svcproc svcproctype="install" graphiccount="0">
            <title>INSTALACI&#211;N</title>
            <step1>
                <para count="1">Instale la nueva junta y aplique sellador.</para>
            </step1>
            <step1>
                <para count="1">Coloque el colector de aceite debajo de la caja del cig&#252;e&#241;al.</para>
            </step1>
            <step1>
                <para count="1">Instale los pernos y espaciadores.</para>
            </step1>
            <step1>
                <para count="1">Apriete los pernos en la secuencia correcta.</para>
            </step1>
        </svcproc>
    </wtsproc>

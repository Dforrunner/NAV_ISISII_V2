<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../isisxslspanish.xsl"?>
<isis lang="es">
    <stitle>Spanish - Procedural Guideline - GRILLETE DEL MUELLE DELANTERO Y/O BUJE, REEMPLAZO</stitle>
    <wtsproc id="w0310003" translation="spanish" wtscode="A03-190" group="03">
        <wtsdescrip>GRILLETE DEL MUELLE DELANTERO Y/O BUJE, REEMPLAZO</wtsdescrip>
        <model>1000</model>
        <modelyr>1996</modelyr>
        <engine>T 444E</engine>
        <tran>International 6+1</tran>
        <config>Modelo RPS</config>
        <svcproc svcproctype="remove" graphiccount="0">
            <title>RETIRO</title>
            <step1>
                <para count="1">Coloque cu&#241;as en las ruedas.</para>
            </step1>
            <step1>
                <para count="1">Levante el soporte del veh&#237;culo con soportes de bastidor.</para>
            </step1>
            <step1>
                <para count="1">Sujete el eje con el gato de piso.</para>
            </step1>
            <step1>
                <para count="1">Quite el perno de ojo del grillete del muelle.</para>
            </step1>
            <step1>
                <para count="1">Quite el perno de ojo del grillete y del suspensor.</para>
            </step1>
            <step1>
                <para count="1">Quite el grillete.</para>
            </step1>
        </svcproc>
        <svcproc svcproctype="install" graphiccount="0">
            <title>INSTALACI&#211;N</title>
            <step1>
                <para count="1">Instale el grillete.</para>
            </step1>
            <step1>
                <para count="1">Instale la tuerca de torsi&#243;n y el perno de ojo del grillete y del suspensor.</para>
            </step1>
            <step1>
                <para count="1">Instale el perno de ojo del grillete del suspensor y la tuerca de torsi&#243;n.</para>
            </step1>
            <step1>
                <para count="1">Quite los soportes del bastidor y baje el veh&#237;culo.</para>
            </step1>
            <step1>
                <para count="1">Retire las cu&#241;as de las ruedas.</para>
            </step1>
        </svcproc>
    </wtsproc>
</isis>

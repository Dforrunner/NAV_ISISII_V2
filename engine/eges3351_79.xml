<isis lang="en">
    <stitle>International&#174; MaxxForce&#8482; DT, 9 and 10 Engine Service Manual - Engine Electrical - Removal -
        Intake Air Heater (IAH) Relay Assembly
    </stitle>
    <svcman/>
    <svcproc svcproctype="remove" graphiccount="2">
        <title>Intake Air Heater (IAH) Relay Assembly</title>
        <figure id="f3550003095" figsize="pg-wide">
            <graphic filename="../figK09/K09003.webp" type="webp" scalefit="1" scalefitwidth="432px" box="no"/>
            <caption>IAH relay assembly - exploded view</caption>
            <calloutgroup>
                <callout1 type="arabic">Mounting bracket</callout1>
                <callout1 type="arabic">M5 x 10 socket screw</callout1>
                <callout1 type="arabic">Junction block</callout1>
                <callout1 type="arabic">Buss bar</callout1>
                <callout1 type="arabic">M6 nut (9)</callout1>
                <callout1 type="arabic">Intake air heater cable (2)</callout1>
                <callout1 type="arabic">High current relay (2)</callout1>
            </calloutgroup>
        </figure>
        <figure id="f3350003094" figsize="col-wide">
            <graphic filename="../figK09/K09010.webp" type="webp" scalefit="1" scalefitwidth="248px" box="no"/>
            <caption>IAH relay assembly</caption>
            <calloutgroup>
                <callout1 type="arabic">M10 nut (2)</callout1>
                <callout1 type="arabic">M8 x 20 bolt</callout1>
                <callout1 type="arabic">Intake air heater grid stud (2)</callout1>
            </calloutgroup>
        </figure>
        <step1>
            <para id="s0315" count="1">Disconnect negative battery cable.</para>
        </step1>
        <step1>
            <para count="1">Remove positive battery cable from IAH relay assembly</para>
        </step1>
        <step1>
            <para count="1">Remove two M6 nuts connecting intake air heater cables to intake air heater grid studs.
            </para>
        </step1>
        <step1>
            <para count="1">Remove M8 x 20 bolt and two M10 nuts holding IAH relay assembly to engine.</para>
        </step1>
        <step1>
            <para count="1">Remove IAH relay assembly.</para>
        </step1>
    </svcproc>

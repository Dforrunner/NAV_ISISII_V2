    <stitle>INTERNATIONAL&#174; VT 365 DIESEL ENGINE - Appendix A - Specifications - Fuel System</stitle>
    <svcman/>
    <svcproc svcproctype="check" graphiccount="0">
        <title>Fuel System</title>
        <table id="te2351410" tblwidth="pg-wide" frame="topbot">
            <tgroup cols="2">
                <colspec colname="col1" colwidth="100.00*"/>
                <colspec colname="col2" colwidth="3.78in"/>
                <spanspec namest="col1" nameend="col2" spanname="SPAN1"/>
                <tbody>
                    <row>
                        <entry colspan="2" spanname="SPAN1">
                            <para>
                                <emph type="bold">Fuel Filter</emph>
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>Type</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>10 micron with water separation</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>Normal fuel pressure (after fuel filter)</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>345 - 482 kPa (50 - 70 psi)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>Fuel pressure in filter not to exceed</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>448 kPa @ 132 L/h (65 psi @ 35 gal/h)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colspan="2" spanname="SPAN1">
                            <para>
                                <emph type="bold">Fuel Pressure Regulating Valve</emph>
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>Valve opening pressure</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>351 &#177; 31 kPa (51 &#177; 4.5 psi)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>Heater element activates</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>3.6 - 10.8 &#186;C (38.5 - 51.5 &#186;F)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>Heater element deactivates</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>19.1 - 29.6 &#186;C (66.5 - 83.5 &#186;F)</para>
                        </entry>
                    </row>
                </tbody>
            </tgroup>
        </table>
    </svcproc>

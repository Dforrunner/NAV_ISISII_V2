                        <row>
                            <entry morerows="4">
                                <para>Engine effects</para>
                            </entry>
                            <entry>
                                <para>Incorrect or inoperative thermostat</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>-</para>
                            </entry>
                            <entry>
                                <para>Failed Turbocharger Boost Control (TCBC) system</para>

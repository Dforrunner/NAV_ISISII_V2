    <stitle>Spanish - Manual de Servicio Para Motores Diesel VT 365 - Especificaciones - Sistema de combustible</stitle>
    <svcman/>
    <svcproc id="s29600appa.1.10" svcproctype="check" graphiccount="0">
        <title>Sistema de combustible</title>
        <table id="t29600appa11" tblwidth="pg-wide" frame="topbot">
            <title>Filtro de combustible y v&#225;lvula reguladora de presi&#243;n</title>
            <tgroup cols="2">
                <colspec colname="col1" colwidth="100.00*"/>
                <colspec colname="col2" colwidth="3.78in"/>
                <spanspec namest="col1" nameend="col2" spanname="SPAN1"/>
                <tbody>
                    <row>
                        <entry colspan="2" spanname="SPAN1">
                            <para>
                                <emph type="bold">Filtro de combustible</emph>
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>Tipo</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>De 10 micrones con separaci&#243;n de agua</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>Presi&#243;n normal del combustible (despu&#233;s del filtro)</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>345 - 482 kPa (50 - 70 lb/pulg
                                <supscript>2</supscript>)
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>La presi&#243;n de combustible en el filtro no debe exceder de:</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>448 kPa a 132 litros/h (65 lb/pulg
                                <supscript>2</supscript>
                                a 35 galones/h)
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry colspan="2" spanname="SPAN1">
                            <para>
                                <emph type="bold">V&#225;lvula reguladora de presi&#243;n</emph>
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>Presi&#243;n de apertura</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>351 &#177; 31 kPa (51 &#177; 4,5 lb/pulg
                                <supscript>2</supscript>)
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>Activaci&#243;n del elemento calentador</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>3,6 - 10,8 &#186;C (38,5 - 51,5 &#186;F)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>Desactivaci&#243;n del elemento calentador</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>19,1 - 29,6 &#186;C (66,5 - 83,5 &#186;F)</para>
                        </entry>
                    </row>
                </tbody>
            </tgroup>
        </table>
    </svcproc>

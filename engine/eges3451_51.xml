    <svcman/>
    <svcproc id="s03.3.4" svcproctype="remove" graphiccount="1">
        <title>Exhaust Back Pressure (EBP) Sensor</title>
        <figure id="f3450003023" figsize="col-wide">
            <graphic filename="../figM09/M09108.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="192px" box="no"/>
            <caption>EBP sensor</caption>
        </figure>
        <step1>
            <para count="1">Disconnect electrical connector from EBP sensor mounted in EBP tube assembly near breather
                support on right valve cover.
            </para>
        </step1>
        <step1>
            <para count="1">Remove EBP sensor.</para>
        </step1>

    <stitle>International&#174; DT 466E, DT 466, 530, 530E, and HT 530 Diesel Engines - Turbochargers - Special
        Information - Special Torque
    </stitle>
    <svcman/>
    <svcproc id="s210010208.2" svcproctype="torque" graphiccount="0">
        <title>Special Torque</title>
        <para id="t210010203"></para>
        <table tblwidth="pg-wide" type="simple" frame="top">
            <tgroup cols="2">
                <colspec colname="col1" colwidth="54.61*"/>
                <colspec colname="col2" colwidth="45.39*"/>
                <spanspec namest="col1" nameend="col2" spanname="span1"/>
                <tbody>
                    <row>
                        <entry colname="col1">
                            <para>Center Housing-to-Turbine Housing Cap screws</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>13 N&#183;m (115 lb&#183;in)</para>
                        </entry>
                    </row>
                    <row>
                        <entry>
                            <para>Wastegate housing to turbine housing cap screws</para>
                        </entry>
                        <entry align="right">
                            <para>21 - 24 N&#183;m (185 - 215 lbf&#183;in)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>Center Housing-to-Compressor Housing Cap screws</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>21 - 24 N&#183;m (185 - 215 lbf&#183;in)</para>
                        </entry>
                    </row>
                    <row>
                        <entry>
                            <para>Compressor housing to back plate</para>
                        </entry>
                        <entry align="right">
                            <para>21 - 24 N&#183;m (185 - 215 lbf&#183;in)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>Turbocharger Mounting Nuts</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>71 N&#183;m (52 lbf&#183;ft)</para>
                        </entry>
                    </row>
                    <row>
                        <entry>
                            <para>EVRT&#174; Turbocharger mounting studs and nuts</para>
                        </entry>
                        <entry align="right">
                            <para>71 N&#183;m (52 lbf&#183;ft)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colspan="2" spanname="span1">
                            <note>
                                <para>Apply clean engine oil to all threads.</para>
                            </note>
                        </entry>
                    </row>
                </tbody>
            </tgroup>
        </table>
    </svcproc>

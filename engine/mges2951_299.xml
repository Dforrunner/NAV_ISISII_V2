    <svcproc id="s29600appa.1.11" svcproctype="check" graphiccount="0">
        <title>Volante, tapa trasera y conjunto de bomba de combustible y servodirecci&#243;n</title>
        <table id="t29600appa12" tblwidth="pg-wide" frame="topbot">
            <title>Volante, tapa trasera y conjunto de bomba de combustible y servodirecci&#243;n</title>
            <tgroup cols="3">
                <colspec colname="col1" colwidth="100.00*"/>
                <colspec colname="col2" colwidth="3.14in"/>
                <colspec colname="col3" colwidth="3.25in"/>
                <spanspec namest="col1" nameend="col2" spanname="SPAN2"/>
                <spanspec namest="col1" nameend="col3" spanname="SPAN1"/>
                <tbody>
                    <row>
                        <entry colspan="3" spanname="SPAN1">
                            <para>
                                <emph type="bold">Volante</emph>
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry colspan="2" spanname="SPAN2">
                            <para>Desviaci&#243;n m&#225;xima de la superficie del volante (transmisi&#243;n mec&#225;nica)</para>
                        </entry>
                        <entry align="right">
                            <para>0,25 mm (0,010")</para>
                        </entry>
                    </row>
                    <row>
                        <entry>
                            <para></para>
                        </entry>
                        <entry>
                            <para>Desviaci&#243;n de la corona del volante (medici&#243;n m&#225;xima total del
                                indicador) (transmisi&#243;n autom&#225;tica)
                            </para>
                        </entry>
                        <entry align="right">
                            <para>1,27 mm (0,050")</para>
                        </entry>
                    </row>
                    <row>
                        <entry colspan="3" spanname="SPAN1">
                            <para>
                                <emph type="bold">Tapa trasera</emph>
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry colspan="2" spanname="SPAN2">
                            <para>Desviaci&#243;n m&#225;xima de la cara de la tapa trasera</para>
                        </entry>
                        <entry align="right">
                            <para>0,51 mm (0,020")</para>
                        </entry>
                    </row>
                    <row>
                        <entry colspan="3" spanname="SPAN1">
                            <para>
                                <emph type="bold">Engranaje impulsor de la bomba de combustible y servodirecci&#243;n
                                </emph>
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry colspan="3" spanname="SPAN1">
                            <para>Juego:</para>
                        </entry>
                    </row>
                    <row>
                        <entry>
                            <para></para>
                        </entry>
                        <entry>
                            <para>Engranaje del cig&#252;e&#241;al a engranaje libre</para>
                        </entry>
                        <entry align="right">
                            <para>0,173 - 0,288 mm (0,0068 - 0,0113")</para>
                        </entry>
                    </row>
                    <row>
                        <entry>
                            <para></para>
                        </entry>
                        <entry>
                            <para>Engranaje libre a engranaje impulsor de la bomba</para>
                        </entry>
                        <entry align="right">
                            <para>0,173 - 0,288 mm (0,0068 - 0,0113")</para>
                        </entry>
                    </row>
                    <row>
                        <entry colspan="3" spanname="SPAN1">
                            <para>
                                <emph type="bold">Bomba de servodirecci&#243;n</emph>
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry colspan="2" spanname="SPAN2">
                            <para>Presi&#243;n m&#225;xima de operaci&#243;n</para>
                        </entry>
                        <entry align="right">
                            <para>17,5 MPa (2538 lb/pulg
                                <supscript>2</supscript>)
                            </para>
                        </entry>
                    </row>

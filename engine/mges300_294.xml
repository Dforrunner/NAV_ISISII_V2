        <title>Sistema de combustible</title>
        <table id="AppA30013A" tblwidth="pg-wide" type="simple" frame="topbot">
            <tgroup cols="2" colsep="0" rowsep="1">
                <colspec colname="col1" colwidth="100.00*"/>

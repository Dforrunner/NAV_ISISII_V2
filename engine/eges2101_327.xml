<isis lang="en">
    <stitle>International&#174; DT 466E, DT 466, 530, 530E, and HT 530 Diesel Engines - Specifications - High-pressure
        Lube Oil System
    </stitle>
    <svcman/>
    <svcproc id="sa2100101.12" graphiccount="0">
        <title>High-pressure Lube Oil System</title>
        <para id="ta210011201"></para>
        <table tblwidth="pg-wide" type="simple" frame="topbot">
            <tgroup cols="2">
                <colspec colname="col1" colwidth="50.12*"/>
                <colspec colname="col2" colwidth="49.88*"/>
                <spanspec namest="col1" nameend="col2" spanname="span1"/>
                <thead>
                    <row>
                        <entry colspan="2" spanname="span1">
                            <para>
                                <title>Injection Pressure Regulator</title>
                            </para>
                        </entry>
                    </row>
                </thead>
                <tbody>
                    <row>
                        <entry colname="col1">
                            <para>Operating temperature range</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>-40 to 125 &#176;C (-40 to 257 &#176;F)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>Maximum flow rate</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>17.5 L/min (4.62 gpm)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>Maximum operating pressure</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>23.5 MPa (3400 psi)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colspan="2" spanname="span1">
                            <para>
                                <title>High-pressure Pump Drive Gear</title>
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>Gear backlash</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>0.140 to 0.256 mm (0.0055 to 0.0101 in)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>End play</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>0.45 to 1.22 mm (0.018 to 0.048 in)</para>
                        </entry>
                    </row>
                </tbody>
            </tgroup>
        </table>
    </svcproc>

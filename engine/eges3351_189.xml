    <svcsubsec>
        <title>Specifications</title>
        <svcproc svcproctype="measure" graphiccount="0">
            <title>Service Procedure</title>
            <para id="t335000701"></para>
            <table id="spec012" tblwidth="pg-wide" type="simple" frame="topbot">
                <tgroup cols="2">
                    <colspec colname="col1" colwidth="1.20*" colsep="0"/>
                    <colspec colname="col2" align="left" colwidth="0.80*"/>
                    <tbody>
                        <row>
                            <entry morerows="1">
                                <para>Fuel heater switching points</para>
                            </entry>
                            <entry>
                                <para>On: 7 &#176;C (44.6 &#176;F)</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>-</para>
                            </entry>
                            <entry>
                                <para>Off: 21 &#176;C (69.8 &#176;F)</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Fuel pressure regulator assembly opening pressure</para>
                            </entry>
                            <entry>
                                <para>420 to 490 kPa (61 to 71 psi)</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Fuel strainer</para>
                            </entry>
                            <entry>
                                <para>150 micron</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>High-pressure oil manifold pressure range</para>
                            </entry>
                            <entry>
                                <para>5 to 32 MPa (725 to 4,650 psi)</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>High-pressure oil pump end play</para>
                            </entry>
                            <entry>
                                <para>0.127 to 0.457 mm (0.005 to 0.018 in)</para>
                            </entry>
                        </row>
                    </tbody>
                </tgroup>
            </table>
        </svcproc>

    <svcsubsec id="s03.1">
        <title>Engine Electronic Components</title>
        <svcproc id="s03.1.1" graphiccount="4">
            <title>Service Procedure</title>
            <figure id="f3450003001" figsize="pg-wide">
                <graphic filename="../figM09/M09113.webp" type="webp" scalefit="1" scalefitwidth="532px"
                         scalefitheight="408px" box="no"/>
                <caption>Electronic components - Front</caption>
                <calloutgroup>
                    <callout1 type="arabic">Engine Fuel Temperature (EFT) sensor</callout1>
                    <callout1 type="arabic">Engine Fuel Pressure (EFP) sensor</callout1>
                    <callout1 type="arabic">Engine Coolant Temperature (ECT) sensor</callout1>
                </calloutgroup>
            </figure>
            <figure id="f3450003002" figsize="pg-wide">
                <graphic filename="../figM09/M09114.webp" type="webp" scalefit="1" scalefitwidth="532px"
                         scalefitheight="408px" box="no"/>
                <caption>Electronic components - Left</caption>
                <calloutgroup>
                    <callout1 type="arabic">Fuel injector assembly</callout1>
                    <callout1 type="arabic">Glow plug</callout1>
                    <callout1 type="arabic">Camshaft Position (CMP) sensor</callout1>
                </calloutgroup>
            </figure>
            <figure id="f3450003003" figsize="pg-wide">
                <graphic filename="../figM09/M09115.webp" type="webp" scalefit="1" scalefitwidth="532px"
                         scalefitheight="408px" box="no"/>
                <caption>Electronic components - Right</caption>
                <calloutgroup>
                    <callout1 type="arabic">Glow plug</callout1>
                    <callout1 type="arabic">Fuel Rail Pressure (FRP) sensor</callout1>
                    <callout1 type="arabic">Fuel injector assembly</callout1>
                    <callout1 type="arabic">Crankshaft Position (CKP) sensor</callout1>
                    <callout1 type="arabic">Water In Fuel (WIF) sensor</callout1>
                    <callout1 type="arabic">Bowl assembly with fuel heater/probe</callout1>
                    <callout1 type="arabic">Coolant heater</callout1>
                </calloutgroup>
            </figure>
            <figure id="f3450003004" figsize="pg-wide">
                <graphic filename="../figM09/M09116.webp" type="webp" scalefit="1" scalefitwidth="532px"
                         scalefitheight="408px" box="no"/>
                <caption>Electronic components - Top</caption>
                <calloutgroup>
                    <callout1 type="arabic">Intake throttle housing assembly</callout1>
                    <callout1 type="arabic">Exhaust Gas Recirculating (EGR) valve</callout1>
                    <callout1 type="arabic">Engine Oil Temperature (EOT) sensor</callout1>
                    <callout1 type="arabic">Engine Oil Pressure (EOP) sensor</callout1>
                    <callout1 type="arabic">Glow plug relay</callout1>
                    <callout1 type="arabic">Exhaust Back Pressure (EBP) sensor</callout1>
                    <callout1 type="arabic">Electronic Control Module (ECM)</callout1>
                    <callout1 type="arabic">Variable Geometry Turbocharger (VGT) actuator</callout1>
                    <callout1 type="arabic">Manifold Absolute Pressure (MAP) sensor</callout1>
                    <callout1 type="arabic">Manifold Air Temperature (MAT) sensor</callout1>
                </calloutgroup>
            </figure>

<isis lang="en">
    <stitle>International&#174; DT 466E, DT 466, 530, 530E, and HT 530 Diesel Engines - Turbochargers - Special
        Information
    </stitle>
    <svcman/>
    <svcsubsec id="s210010208">
        <title>Special Information</title>
        <svcproc id="s210010208.1" svcproctype="describe" graphiccount="0">
            <title>Specifications</title>
            <para id="t210010201"></para>
            <table tblwidth="pg-wide" type="simple" frame="topbot">
                <tgroup cols="2">
                    <colspec colname="col1" colwidth="51.02*"/>
                    <colspec colname="col2" align="right" colwidth="48.98*"/>
                    <spanspec namest="col1" nameend="col2" spanname="SPAN1"/>
                    <tbody>
                        <row>
                            <entry colname="col1">
                                <para>Turbine shaft axial end play</para>
                            </entry>
                            <entry align="right" colname="col2">
                                <para>0.02 to 0.10 mm (0.001 to 0.004 in)</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="col1">
                                <para>Turbine shaft radial movement (play)</para>
                            </entry>
                            <entry align="right" colname="col2">
                                <para>0.08 to 0.15 mm (0.003 to 0.006 in)</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="col1">
                                <para>Wastegate actuator movement</para>
                            </entry>
                            <entry align="right" colname="col2">
                                <para>0.37 mm at 196 kPa (0.015 in at 29 psi)</para>
                            </entry>
                        </row>
                        <row>
                            <entry colspan="2" spanname="SPAN1">
                                <para>
                                    <title>EVRT&#174;</title>
                                </para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Turbine shaft axial end play</para>
                            </entry>
                            <entry align="right">
                                <para>0.05 to 0.13 mm (0.002 to 0.005 in)</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Turbine shaft radial movement (play)</para>
                            </entry>
                            <entry align="right">
                                <para>0.52 to 0.74 mm (0.020 to 0.029 in)</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>EVRT&#174; axial linkage shaft</para>
                            </entry>
                            <entry align="right">
                                <para>Must strike open and closed stops in actuator, 90&#176; rotation</para>
                            </entry>
                        </row>
                    </tbody>
                </tgroup>
            </table>
        </svcproc>
    </svcsubsec>

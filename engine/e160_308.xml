<isis lang="en">
    <stitle>DT-466E and 530E Diesel Engine Service (1995 through 1999) - SPECIFICATIONS, SPECIAL TORQUE VALUES, AND
        SPECIAL TOOLS - TURBOCHARGER - Special Torque Values
    </stitle>
    <svcman/>
    <svcproc svcproctype="describe" graphiccount="0">
        <title>Special Torque Values</title>
        <table id="t1600202" tblwidth="pg-wide">
            <title>Torque values</title>
            <tgroup cols="2">
                <colspec colname="1" colwidth=""/>
                <colspec colname="2" colwidth=""/>
                <tbody>
                    <row>
                        <entry colname="1">
                            <para>Back Plate-to-Center Housing Capscrews</para>
                        </entry>
                        <entry colname="2">
                            <para>21-24 Nm (185-215 lb-in.)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="1">
                            <para>Center Housing-to-Turbine Housing Capscrews</para>
                        </entry>
                        <entry colname="2">
                            <para>13.5 Nm (115 lb-in.)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="1">
                            <para>Center Housing-to-Compressor Housing Capscrews</para>
                        </entry>
                        <entry colname="2">
                            <para>21-24 Nm (185-215 lb-in.)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="1">
                            <para>Compressor Wheel (Non-Wastegate Turbocharger) (See
                                <emph type="bold">NOTE</emph>)
                            </para>
                        </entry>
                        <entry colname="2">
                            <para>13.5-15.2 Nm (115-135 lb-in.)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="1">
                            <para>Compressor Wheel (Wastegate Turbocharger) (See
                                <emph type="bold">NOTE</emph>)
                            </para>
                        </entry>
                        <entry colname="2">
                            <para>10-12.5 Nm (90-111 lb-in.)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="1">
                            <para>Turbocharger Mounting Nuts</para>
                        </entry>
                        <entry colname="2">
                            <para>66 Nm (49 lb-ft.)</para>
                        </entry>
                    </row>
                </tbody>
            </tgroup>
        </table>
        <specpara>
            <note>
                <para>Apply clean engine oil to threads of bolt.</para>
            </note>
        </specpara>
    </svcproc>

                    <row>
                        <entry>
                            <para>Piston pin bushing inside diameter</para>
                        </entry>
                        <entry>
                            <para>46.393 to 46.401 mm (1.8265 to 1.8268 in)</para>
                        </entry>
                    </row>
                    <row>
                        <entry>
                            <para>Twist (maximum)</para>
                        </entry>
                        <entry>
                            <para>0.051 mm (0.002 in)</para>
                        </entry>
                    </row>
                </tbody>
            </tgroup>

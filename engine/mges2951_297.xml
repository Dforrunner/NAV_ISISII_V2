    <svcproc id="s29600appa.1.9" svcproctype="check" graphiccount="0">
        <title>Bomba de aceite de alta presi&#243;n</title>
        <table id="t29600appa10" tblwidth="pg-wide" frame="topbot">
            <title>IPR (regulador de la presi&#243;n de inyecci&#243;n)</title>
            <tgroup cols="2" colsep="0" rowsep="1">
                <colspec colname="col1" colwidth="100.00*"/>
                <colspec colname="col2" colwidth="3.59in"/>
                <spanspec namest="col1" nameend="col2" spanname="1to2"/>
                <tbody>
                    <row>
                        <entry colspan="2" spanname="1to2">
                            <para>
                                <emph type="bold">ICP (sensor de la presi&#243;n de control de inyecci&#243;n)</emph>
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>L&#237;mites de presi&#243;n de operaci&#243;n</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>0 - 30 MPa (0 - 4350 lb/pulg
                                <supscript>2</supscript>)
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry colspan="2" spanname="1to2">
                            <para>
                                <emph type="bold">IPR (regulador de la presi&#243;n de inyecci&#243;n)</emph>
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry>
                            <para>Presi&#243;n de apertura</para>
                        </entry>
                        <entry align="right">
                            <para>31 MPa (4500 lb/pulg
                                <supscript>2</supscript>)
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry>
                            <para>Presi&#243;n m&#225;xima</para>
                        </entry>
                        <entry align="right">
                            <para>38 MPa (5500 lb/pulg
                                <supscript>2</supscript>)
                            </para>
                        </entry>
                    </row>
                </tbody>
            </tgroup>
        </table>

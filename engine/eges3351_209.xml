    <svcsubsec>
        <title>Special Torque</title>
        <svcproc svcproctype="torque" graphiccount="0">
            <title>Service Procedure</title>
            <para id="t335000802"></para>
            <table id="torq014" tblwidth="pg-wide" type="simple" frame="topbot">
                <tgroup cols="2">
                    <colspec colname="col1" colwidth="1.19*"/>
                    <colspec colname="col2" align="justify" colwidth="0.81*"/>
                    <spanspec namest="col1" nameend="col2" spanname="SPAN1"/>
                    <tbody>
                        <row>
                            <entry>
                                <para>Brake Control Pressure (BCP) sensor and Injection Control Pressure (ICP) sensor
                                </para>
                            </entry>
                            <entry align="justify">
                                <para>18 N&#183;m (162 lbf&#183;in)</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Brake actuator piston locknut</para>
                            </entry>
                            <entry align="justify">
                                <para>25 N&#183;m (19 lbf&#183;ft)</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Brake shut-off valve solenoid nut</para>
                            </entry>
                            <entry align="justify">
                                <para>5 N&#183;m (45 lbf&#183;in)</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Brake shut-off valve</para>
                            </entry>
                            <entry align="justify">
                                <para>27 N&#183;m (20 lbf&#183;ft)</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>High-pressure oil manifold bolts M8 x 90</para>
                            </entry>
                            <entry align="justify">
                                <para>See
                                    <hotlnk document="eges3351_204.xml" xrefid="s0811.1">High-pressure Oil Manifold
                                        (Brake)
                                    </hotlnk>
                                    <xref refid="s0811.1"/>.
                                </para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Injector oil inlet adaptor assembly</para>
                            </entry>
                            <entry align="justify">
                                <para>204 N&#183;m (150 lbf&#183;ft)</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Oil pressure relief valve</para>
                            </entry>
                            <entry align="justify">
                                <para>45 N&#183;m (33 lbf&#183;ft)</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Plug assembly, M12</para>
                            </entry>
                            <entry align="justify">
                                <para>12 N&#183;m (108 lbf&#183;in)</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Plug assembly, M10</para>
                            </entry>
                            <entry align="justify">
                                <para>12 N&#183;m (108 lbf&#183;in)</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Rail end plug assembly</para>
                            </entry>
                            <entry align="justify">
                                <para>204 N&#183;m (150 lbf&#183;ft)</para>
                            </entry>
                        </row>
                    </tbody>
                </tgroup>
            </table>

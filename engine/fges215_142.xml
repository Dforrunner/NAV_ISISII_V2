    <stitle>French - Manuel de diagnostic - R&#201;PARATION DU CONNECTEUR ET DU FAISCEAU - REMPLACEMENT D'UNE BORNE
    </stitle>
    <svcman/>
    <svcproc svcproctype="describe" graphiccount="3">
        <title>REMPLACEMENT D'UNE BORNE</title>
        <step1>
            <para count="1">Enlever l'isolant, en prenant soin ne pas couper de fils.</para>
        </step1>
        <step1>
            <para count="1">Placer le fil dans la nouvelle borne
                <xref refid="f1750623"/>.
            </para>
            <figure id="f1750623" figsize="col-wide">
                <graphic filename="../figeg5/eg5473.webp" type="webp" scalefit="1" scalefitwidth="248px"/>
                <caption>Placer la borne sur le fil</caption>
            </figure>
        </step1>
        <step1>
            <para count="1">Sertir &#224; la main les ailettes de l'&#226;me d'abord, puis les ailettes de l'isolant
                <xref refid="f1750624"/>.
            </para>
            <figure id="f1750624" figsize="col-wide">
                <graphic filename="../figeg5/eg5474.webp" type="webp" scalefit="1" scalefitwidth="248px"/>
                <caption>Borne sur fil</caption>
            </figure>
        </step1>
        <step1>
            <para count="1">Sertissage du contact termin&#233;
                <xref refid="f1750625"/>.
            </para>
            <figure id="f1750625" figsize="col-wide">
                <graphic filename="../figeg5/eg5475.webp" type="webp" scalefit="1" scalefitwidth="248px"/>
                <caption>Sertissage du contact termin&#233;</caption>
            </figure>
        </step1>
    </svcproc>

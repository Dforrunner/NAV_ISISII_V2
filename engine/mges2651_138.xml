    <svcman/>
    <svcproc graphiccount="3">
        <title>Carcasa de suministro de refrigerante</title>
        <figure id="f2650660" figsize="col-wide">
            <graphic filename="../figh05/h05025.webp" type="webp" scalefit="1" scalefitwidth="248px"/>
            <caption>Carcasa de suministro y sello del orificio de refrigerante</caption>
        </figure>
        <step1>
            <para count="1">Ponga el sello del orificio dentro de la cavidad fresada correspondiente en la carcasa de
                suministro de refrigerante.
            </para>
        </step1>
        <figure id="f2650661" figsize="col-wide">
            <graphic filename="../figh05/h05013.webp" type="webp" scalefit="1" scalefitwidth="248px"/>
            <caption>Pernos de la carcasa de suministro de refrigerante</caption>
            <calloutgroup>
                <callout1 type="arabic">Perno M10 x 25 (4)</callout1>
            </calloutgroup>
        </figure>
        <step1>
            <para count="1">Ponga los cuatro pernos (M10 x 25) de la carcasa de suministro de refrigerante.</para>
        </step1>
        <figure id="f2650662" figsize="col-wide">
            <graphic filename="../figh05/h05026.webp" type="webp" scalefit="1" scalefitwidth="248px"/>
            <caption>Ajuste de los pernos de la carcasa de suministro de refrigerante</caption>
        </figure>
        <step1>
            <para count="1">Ajuste los pernos al
                <hotlnk document="mges2651_S265APPB01.xml" xrefid="S265APPB01" appearance="hottext">torque
                    est&#225;ndar</hotlnk>.
            </para>
        </step1>

    <stitle>VT 275 Engine Service Manual - Appendix A - Specifications - Specifications - High-pressure Oil Pump
    </stitle>
    <svcman/>
    <svcproc graphiccount="0">
        <title>High-pressure Oil Pump</title>
        <table id="AppA30012A" tblwidth="pg-wide" type="simple" frame="topbot">
            <tgroup cols="2" colsep="0" rowsep="1">
                <colspec colname="col1" colwidth="100.00*"/>
                <colspec colname="col2" align="left" colwidth="2.15in"/>
                <spanspec namest="col1" nameend="col2" spanname="1to2"/>
                <tbody>
                    <row>
                        <entry colspan="2" spanname="1to2">
                            <para id="PappA30012A">
                                <title>IPR Valve and ICP Sensor</title>
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry colspan="2" spanname="1to2">
                            <para>
                                <emph type="bold">Injection Control Pressure (ICP) Sensor</emph>
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>Operating pressure range</para>
                        </entry>
                        <entry colname="col2">
                            <para>0 to 30 MPa (0 to 4,350 psi)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colspan="2" spanname="1to2">
                            <para>
                                <emph type="bold">Injection Pressure Regulator (IPR) Valve</emph>
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry>
                            <para>IPR valve relief pressure</para>
                        </entry>
                        <entry>
                            <para>31 MPa (4,500 psi)</para>
                        </entry>
                    </row>
                    <row>
                        <entry>
                            <para>IPR valve maximum pressure</para>
                        </entry>
                        <entry>
                            <para>38 MPa (5,500 psi)</para>
                        </entry>
                    </row>
                </tbody>
            </tgroup>
        </table>

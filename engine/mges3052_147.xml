                                    <title>Punto de prueba</title>
                                </para>
                            </entry>
                            <entry colname="col2">
                                <para>
                                    <title>Requisito</title>
                                </para>
                            </entry>
                            <entry colname="col3">
                                <para>
                                    <title>Comentarios</title>
                                </para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="col1">
                                <para>1 a tierra</para>
                            </entry>
                            <entry colname="col2">
                                <para>&#60; 5 &#x2126;</para>
                            </entry>

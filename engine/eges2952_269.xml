<isis lang="en">
    <stitle>VT 365 Diesel Engine - 2004 Model Year - FUEL SYSTEM - Installation - Fuel Injectors</stitle>
    <svcman/>
    <svcproc id="s26512.3.5" svcproctype="install" graphiccount="0">
        <title>Fuel Injectors</title>
        <specpara>
            <note>
                <para>See the "Cylinder Head and Valve Train" section for installation procedures.</para>
            </note>
        </specpara>
    </svcproc>
    <spec>
        <geninfo>
            <table id="te2651201" tblwidth="pg-wide" frame="topbot">
                <title>Fuel Filter and Pressure Regulating Valve</title>
                <tgroup cols="2" colsep="0" rowsep="1">
                    <colspec colname="col1" colwidth="100.00*"/>
                    <colspec colname="col2" colwidth="3.78in"/>
                    <spanspec namest="col1" nameend="col2" spanname="1to2"/>
                    <tbody>
                        <row>
                            <entry colspan="2" spanname="1to2">
                                <para>
                                    <emph type="bold">Fuel Filter</emph>
                                </para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="col1">
                                <para>Type</para>
                            </entry>
                            <entry colname="col2" align="right">
                                <para>10 micron with water separation</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="col1">
                                <para>Normal fuel pressure (after fuel filter)</para>
                            </entry>
                            <entry colname="col2" align="right">
                                <para>345 to 482 kPa (50 to 70 psi)</para>
                            </entry>
                        </row>
                        <row>
                            <entry colspan="2" spanname="1to2">
                                <para>
                                    <emph type="bold">Fuel Pressure Regulating Valve</emph>
                                </para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Valve opening pressure</para>
                            </entry>
                            <entry align="right">
                                <para>351 &#177; 31 kPa (51 &#177; 5 psi)</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Heater element activates</para>
                            </entry>
                            <entry align="right">
                                <para>3.6 to 10.8&#176;C (38.5 to 51.5&#176;F)</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Heater element deactivates</para>
                            </entry>
                            <entry align="right">
                                <para>19.1 to 29.6&#176;C (66.5 to 83.5&#176;F)</para>
                            </entry>
                        </row>
                    </tbody>
                </tgroup>
            </table>
        </geninfo>
    </spec>

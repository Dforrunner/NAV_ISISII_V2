    <stitle>DT/DTA-466 Diesel Engine Service (1990) - TURBOCHARGERS - SPECIAL TORQUES</stitle>
    <svcman/>
    <svcsubsec>
        <title>SPECIAL TORQUES</title>
        <svcproc svcproctype="describe" graphiccount="0">
            <title>Model T04E and T04B</title>
            <table id="t4150202">
                <tgroup cols="2">
                    <colspec colname="1" colwidth="180p"/>
                    <colspec colname="2" colwidth="216p"/>
                    <tbody>
                        <row>
                            <entry colname="1">
                                <para>Back Plate-to-Center Housing Capscrews:</para>
                            </entry>
                            <entry colname="2">
                                <para></para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>T04E and Current T04B</para>
                            </entry>
                            <entry colname="2">
                                <para>12-14 Nm (110-125 Ib-in.)</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>Early T04B (Refer to
                                    <emph type="bold">Figure
                                        <hotlnk document="e415_17.xml" xrefid="ef4150236">2.36</hotlnk>
                                    </emph>
                                    )
                                </para>
                            </entry>
                            <entry colname="2">
                                <para>5.65 Nm (50 Ib-in.)</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>Impeller Shaft Locknut (Tighten finger tight plus 90&#176;):</para>
                            </entry>
                            <entry colname="2">
                                <para></para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>(Use tee handle wrench only)</para>
                            </entry>
                            <entry colname="2">
                                <para>2.15 Nm (19 Ib-in.)</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>Center Housing-to-Turbine Housing Capscrews</para>
                            </entry>
                            <entry colname="2">
                                <para>16-19 Nm (140-170 lb-in.)</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>Center Housing-to-Compressor Housing Capscrews:</para>
                            </entry>
                            <entry colname="2">
                                <para></para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>T04E</para>
                            </entry>
                            <entry colname="2">
                                <para>16.9 Nm (150 lb-in.)</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>T04B</para>
                            </entry>
                            <entry colname="2">
                                <para>15-19 Nm (135-165 lb-in.)</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>Turbocharger Mounting Bolts/Nuts</para>
                            </entry>
                            <entry colname="2">
                                <para>27 Nm (20 lb-ft.)</para>
                            </entry>
                        </row>
                    </tbody>
                </tgroup>
            </table>
        </svcproc>

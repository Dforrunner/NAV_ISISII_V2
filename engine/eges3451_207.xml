    <stitle>International&#174; MaxxForce&#8482; 7 Engine Service Manual - Cylinder Head and Valve Train - Removal -
        Valve Cover Base
    </stitle>
    <svcman/>
    <svcproc id="s10.2.4" svcproctype="remove" graphiccount="2">
        <title>Valve Cover Base</title>
        <figure id="f3450010025" figsize="col-wide">
            <graphic filename="../figM08/M08119.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="192px"/>
            <caption>Valve cover base assembly (typical)</caption>
            <calloutgroup>
                <callout1 type="arabic">M6 x 30 bolt (11)</callout1>
                <callout1 type="arabic">Valve cover base assembly</callout1>
            </calloutgroup>
        </figure>
        <step1>
            <para count="1">Remove 11 M6 x 30 bolts.</para>
        </step1>
        <step1>
            <para count="1">Remove valve cover base assembly.</para>
        </step1>
        <figure id="f3450010026" figsize="col-wide">
            <graphic filename="../figM08/M08138.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="192px"/>
            <caption>Valve cover base gasket (typical)</caption>
        </figure>
        <step1>
            <para count="1">Remove and discard valve cover base gasket.</para>
        </step1>
    </svcproc>

<isis lang="en">
    <stitle>DT-466E and 530E Diesel Engine Service (1995 through 1999) - SPECIFICATIONS, SPECIAL TORQUE VALUES, AND
        SPECIAL TOOLS - ENGINE ELECTRICAL - Special Torque Values
    </stitle>
    <svcman/>
    <svcproc svcproctype="describe" graphiccount="0">
        <title>Special Torque Values</title>
        <table id="t1601101" tblwidth="pg-wide">
            <title>Torque values</title>
            <tgroup cols="2">
                <colspec colname="1" colwidth=""/>
                <colspec colname="2" colwidth=""/>
                <spanspec namest="1" nameend="2" spanname="s1t2"/>
                <tbody>
                    <row>
                        <entry colname="1">
                            <para>Injection Control Pressure Sensor (ICP)
                                <emph type="bold">(See Note)</emph>
                            </para>
                        </entry>
                        <entry colname="2">
                            <para>26 Nm (19 lb-ft)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colspan="2" spanname="s1t2">
                            <para>Front And Rear Engine Mounted ECM:</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="1">
                            <para>Electronic Control Module (ECM) Connector Retaining Screw</para>
                        </entry>
                        <entry colname="2">
                            <para>3.9 Nm (35 in.-lb.)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="1">
                            <para>ECM Mounting Bolts</para>
                        </entry>
                        <entry colname="2">
                            <para>15 Nm (20 lb-ft.)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="1">
                            <para>ECM Mounting Bracket Mounting Bolts</para>
                        </entry>
                        <entry colname="2">
                            <para>15 Nm (20 lb-ft.)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="1">
                            <para>ECM Mounting Bracket Mounting Nuts</para>
                        </entry>
                        <entry colname="2">
                            <para>15 Nm (20 lb-ft.)</para>
                        </entry>
                    </row>
                </tbody>
            </tgroup>
        </table>
        <specpara>
            <note>
                <para>Apply Loctite(r) #277 to threads prior to assembly.</para>
            </note>
        </specpara>
    </svcproc>

                        <entry colname="1" morerows="0">
                            <para></para>
                        </entry>
                        <entry colspan="3" spanname="S2T4" morerows="0">
                            <para>&#8226; Monitor the data while cranking the engine</para>
                            <para>for 20 seconds minimum</para>
                        </entry>
                        <entry colname="5" morerows="0">
                            <para></para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry colname="1" morerows="5">
                            <para></para>
                        </entry>
                        <entry colname="2" morerows="0">
                            <para>Data</para>
                        </entry>

    </stitle>
    <svcman/>
    <svcproc id="s13.2.3" svcproctype="remove" graphiccount="2">
        <title>Oil Pickup Tube</title>
        <figure id="f3450013005" figsize="col-wide">
            <graphic filename="../figM06/M06102.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="192px" box="no"/>
            <caption>Oil pickup tube</caption>
            <calloutgroup>
                <callout1 type="arabic">M6 X 16 bolt (3)</callout1>
                <callout1 type="arabic">Oil pickup tube</callout1>
            </calloutgroup>
        </figure>
        <step1>
            <para count="1">Remove three M6 x 16 bolts and oil pickup tube.</para>
        </step1>
        <figure id="f3450013006" figsize="col-wide">
            <graphic filename="../figM06/M06103.webp" type="webp" scalefit="1" scalefitwidth="248px"
                     scalefitheight="192px" box="no"/>
            <caption>Oil pickup tube O-ring</caption>
        </figure>
        <step1>
            <para count="1">Remove and discard oil pickup tube O-ring.</para>
        </step1>
    </svcproc>

<isis lang="en">
    <stitle>International&#174; DT 466E, DT 466, 530, 530E, and HT 530 Diesel Engines - Lubricating Oil Pump, Oil
        Filter, and Cooler - Special Information
    </stitle>
    <svcman/>
    <svcsubsec id="s210010906">
        <title>Special Information</title>
        <svcproc id="s210010906.1" svcproctype="describe" graphiccount="0">
            <title>Specifications</title>
            <para id="t210010902"></para>
            <table tblwidth="pg-wide" type="simple" frame="topbot">
                <tgroup cols="2">
                    <colspec colname="col1" colwidth="54.04*"/>
                    <colspec colname="col2" colwidth="45.96*"/>
                    <spanspec namest="col1" nameend="col2" spanname="SPAN1"/>
                    <thead>
                        <row>
                            <entry colspan="2" spanname="SPAN1">
                                <para>
                                    <title>Oil Pump</title>
                                </para>
                            </entry>
                        </row>
                    </thead>
                    <tbody>
                        <row>
                            <entry colname="col1">
                                <para>Type</para>
                            </entry>
                            <entry colname="col2" align="right">
                                <para>Gerotor</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="col1">
                                <para>Drive</para>
                            </entry>
                            <entry colname="col2" align="right">
                                <para>Crankshaft</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="col1">
                                <para>End clearance inner and outer</para>
                            </entry>
                            <entry colname="col2" align="right">
                                <para>0.066 to 0.142 mm (0.0026 to 0.0056 in)</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="col1">
                                <para>Diametrical clearance between outer rotor housing</para>
                            </entry>
                            <entry colname="col2" align="right">
                                <para>0.470 to 0.622 mm (0.0185 to 0.0245 in)</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="col1">
                                <para>Minimum engine oil pressure at low idle speed (700 rpm)
                                    <ftnote id="fn3">Run engine until normal operating temperature is reached. Take oil
                                        pressure measurement with an oil gallery tap.
                                    </ftnote>
                                </para>
                            </entry>
                            <entry colname="col2" align="right">
                                <para>103 kPa (15 psi)</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="col1">
                                <para>Minimum engine oil pressure at high idle speed*</para>
                            </entry>
                            <entry colname="col2" align="right">
                                <para>276 to 483 kPa (40 to 70 psi)</para>
                            </entry>
                        </row>
                        <row>
                            <entry colspan="2" spanname="SPAN1">
                                <para>
                                    <title>Pressure Regulator Valve Assembly</title>
                                </para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="col1">
                                <para>Location</para>
                            </entry>
                            <entry colname="col2" align="right">
                                <para>In crankcase, behind filter header</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="col1">
                                <para>Setting</para>
                            </entry>
                            <entry colname="col2" align="right">
                                <para>331 kPa (48 psi)</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="col1">
                                <para>Valve assembly diameter</para>
                            </entry>
                            <entry colname="col2" align="right">
                                <para>26.949 &#177; 0.038 mm (1.0610 &#177; 0.0015 in)</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="col1">
                                <para>Valve clearance in bore</para>
                            </entry>
                            <entry colname="col2" align="right">
                                <para>0.038 to 0.089 mm (0.0015 to 0.0035 in)</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="col1">
                                <para>Crankcase bore diameter</para>
                            </entry>
                            <entry colname="col2" align="right">
                                <para>27.00 to 27.05 mm (1.063 to 1.065 in)</para>
                            </entry>
                        </row>
                    </tbody>
                </tgroup>
            </table>
        </svcproc>
    </svcsubsec>

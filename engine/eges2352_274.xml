    <stitle>INTERNATIONAL&#174; VT 365 DIESEL ENGINE - Appendix A - Specifications - High Pressure Oil Pump</stitle>
    <svcman/>
    <svcproc svcproctype="check" graphiccount="0">
        <title>High Pressure Oil Pump</title>
        <table id="te2351409" tblwidth="pg-wide" frame="topbot">
            <tgroup cols="2">
                <colspec colname="col1" colwidth="100.00*"/>
                <colspec colname="col2" colwidth="3.59in"/>
                <spanspec namest="col1" nameend="col2" spanname="SPAN1"/>
                <tbody>
                    <row>
                        <entry colspan="2" spanname="SPAN1">
                            <para>
                                <emph type="bold">Hydraulic Oil Pump</emph>
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>Gear backlash</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>0.179 - 0.315 mm (0.007 - 0.0124 in)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colspan="2" spanname="SPAN1">
                            <para>
                                <emph type="bold">Injection Control Pressure Regulator</emph>
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>Operating pressure range</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>3.5 - 20.7 MPa (500 - 3,000 psi)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>IPR valve cracking pressure</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>31 MPa (4,500 psi)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>IPR valve maximum pressure</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>37 MPa (5,400 psi)</para>
                        </entry>
                    </row>
                </tbody>
            </tgroup>
        </table>

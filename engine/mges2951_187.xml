    <stitle>Spanish - Manual de Servicio Para Motores Diesel VT 365 - ENFRIADOR DE ACEITE Y CARCASA DEL FILTRO - Torque
        Chart
    </stitle>
    <svcman/>
    <torque torquetype="special" id="s2960009.6">
        <table id="t296000902" tblwidth="pg-wide" frame="topbot">
            <title>Enfriador de aceite y filtro de aceite</title>
            <tgroup cols="2">
                <colspec colname="col1" colwidth="2.80in"/>
                <colspec colname="col2" colwidth="100.00*"/>
                <tbody>
                    <row>
                        <entry colname="col1">
                            <para>Cubierta del orificio de suministro de refrigerante al enfriador EGR (M6)</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>10 +1/-2 N&#183;m (84 &#177; 12 lbf/pulg)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>Tornillos de montaje del enfriador de aceite (M6 x 23)</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>10 +1/-2 N&#183;m (84 &#177; 12 lbf/pulg)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1">
                            <para>Tornillos de montaje del enfriador de aceite (M8 x 23)</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>22 &#177; 2 N&#183;m (17 &#177; 1 lbf/pie)</para>
                        </entry>
                    </row>
                    <row>
                        <entry colname="col1" morerows="1">
                            <para>Tornillo del tubo de retorno del filtro de aceite</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>
                                <emph type="bold">Placa de base nueva:</emph>
                                6 N&#183;m (50 lbf/pulg)
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry>
                            <para>-</para>
                        </entry>
                        <entry colname="col2" align="right">
                            <para>
                                <emph type="bold">Reinstalaci&#243;n:</emph>
                                3 N&#183;m (23 lbf/pulg)
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry>
                            <para>Tapa del filtro de aceite</para>
                        </entry>
                        <entry align="right">
                            <para>25 N&#183;m (18 lbf/pie)</para>
                        </entry>
                    </row>
                </tbody>
            </tgroup>
        </table>
    </torque>

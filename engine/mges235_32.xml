                    <callout1 type="arabic">Sellos anulares-O (2)</callout1>
                    <callout1 type="arabic">Eje doble com&#250;n</callout1>
                    <callout1 type="arabic">Brida del eje doble com&#250;n</callout1>
                </calloutgroup>
            </figure>
        </svcproc>

                <colspec colname="1" colwidth="54p"/>
                <colspec colname="2" colwidth="54p"/>
                <colspec colname="3" colwidth="200p"/>
                <spanspec namest="1" nameend="3" spanname="s1t3"/>
                <tbody>
                    <row>
                        <entry colspan="3" spanname="s1t3">
                            <para>
                                <emph type="bold">NOTA:</emph>
                                <emph type="underscr">Despu&#233;s de retirar el conector, revise siempre si hay
                                    clavijas da&#241;adas, corrosi&#243;n, terminales sueltos, etc.
                                </emph>
                            </para>
                        </entry>
                    </row>
                    <row>
                        <entry colspan="3" spanname="s1t3">
                            <para>

<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../isisxsl.xsl"?>
<isis lang="en">
    <stitle>ELECTRICAL SYSTEM TROUBLESHOOTING GUIDE - 9800 - CLUTCH CONTROL LINKAGE - 5000, 9300 AND 9400 SERIES -
        TROUBLESHOOTING
    </stitle>
    <svcsection/>
    <svcproc svcproctype="troubleshoot" graphiccount="0">
        <title>TROUBLESHOOTING</title>
        <table id="t1100801">
            <title>TROUBLESHOOTING</title>
            <tgroup cols="2">
                <colspec colnum="1" colname="1" colwidth="1.99in"/>
                <colspec colnum="2" colname="2" colwidth="4.98in" colsep="1"/>
                <thead valign="bottom">
                    <row rowsep="0">
                        <entry>
                            <para>Condition</para>
                        </entry>
                        <entry>
                            <para>Possible Cause</para>
                        </entry>
                    </row>
                </thead>
                <tbody valign="top">
                    <row rowsep="0">
                        <entry>
                            <para>Clutch won't release</para>
                        </entry>
                        <entry>
                            <para>1. Improper adjustments.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>2. Driven disc hub binding on main drive gear splines.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>3. Faulty clutch assembly.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>Clutch dragging</para>
                        </entry>
                        <entry>
                            <para>1. See causes for "Clutch won't release."</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>2. Dirt or foreign material in clutch.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>3. Oil or grease on driven disc linings.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>4. Misalignment.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>5. Broken driven disc linings.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>6. Warped or damaged driven disc, pressure plate or flywheel.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>Clutch slipping</para>
                        </entry>
                        <entry>
                            <para>1. Improper adjustment.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>2. Oil or grease on driven disc linings.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>3. Worn driven disc linings.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>4. Weak or broken clutch pressure plate spring.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>Grabby clutch</para>
                        </entry>
                        <entry>
                            <para>1. Oil or grease on driven disc linings.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>2. Incorrect type of lining material.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>3. Clutch assembly loose or out of parallel with flywheel face.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>4. Warped or damaged driven disc.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>5. Loose engine mounting bolts.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>Noisy clutch</para>
                        </entry>
                        <entry>
                            <para>1. Improper adjustment.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>2. Worn or damaged release bearing.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>3. Clutch assembly loose on flywheel.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>4. Faulty clutch assembly.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>5. Engine/transmission misaligned.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>6. Loss of tension in driven disc cushioning springs</para>
                            <para>(where used).</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>Premature clutch wear</para>
                        </entry>
                        <entry>
                            <para>1. Abusive driving habits.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>2. Improper adjustment.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>3. Incorrect type or improperly installed driven disc facings.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>4. Weak or broken clutch pressure plate springs.</para>
                        </entry>
                    </row>
                    <row rowsep="1">
                        <entry>
                            <para>&#160;</para>
                        </entry>
                        <entry>
                            <para>5. Warped clutch pressure plate.</para>
                        </entry>
                    </row>
                </tbody>
            </tgroup>
        </table>
    </svcproc>

<isis lang="en">
    <stitle>ELECTRICAL SYSTEM TROUBLESHOOTING GUIDE - 9800 - 5 SPEED TRANSMISSIONS SPICER ES43-5 AND ES53-5 -
        SPECIFICATIONS
    </stitle>
    <svcsection/>
    <svcsubsec id="hl1300907">
        <title>SPECIFICATIONS</title>
        <svcsubsecinfo>
            <table id="hl1300908">
                <title>Specifications</title>
                <tgroup cols="2" colsep="1" rowsep="0">
                    <colspec colname="1" colwidth="2.49in"/>
                    <colspec colname="2" colwidth="2.99in"/>
                    <tbody>
                        <row>
                            <entry>
                                <para>Speeds</para>
                            </entry>
                            <entry>
                                <para>5 forward, 1 reverse</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Synchronized</para>
                            </entry>
                            <entry>
                                <para>Gears 2 - 5</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Torque Capacity</para>
                            </entry>
                            <entry>
                                <para>13DBA, 13DBB 430 ft-lbs. (582 N&#8226;m), 13DBC, 13DBD 530 ft-lbs. (718
                                    N&#8226;m)
                                </para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Length</para>
                            </entry>
                            <entry>
                                <para>27" with No.2 clutch housing</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Weight</para>
                            </entry>
                            <entry>
                                <para>325 lbs. (148 kg)</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Clutch Housing</para>
                            </entry>
                            <entry>
                                <para>SAE No.2</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Clutch</para>
                            </entry>
                            <entry>
                                <para>13DBA, 13DBB, 13DBC, 13DBD 12.2 &#38; 14" 1-Plate, push, 13DBC, 13DBD 14" 1-Plate
                                    or 14" 2-Plate, stamped, pull
                                </para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Yoke or Flange</para>
                            </entry>
                            <entry>
                                <para>1480, 1550, 1590, 1610 series</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Drive Gear</para>
                            </entry>
                            <entry>
                                <para>13DBA, 13DBB 1-3/8" or 1-1/2" 10 spline, 13DBC, 13DBD 1-1/2" or 1-3/4" 10 spline
                                </para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Output Shaft</para>
                            </entry>
                            <entry>
                                <para>2" - 38 involute spline</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Oil Capacity</para>
                            </entry>
                            <entry>
                                <para>Refer to CTS-5065K</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Brake</para>
                            </entry>
                            <entry>
                                <para>Optional mounting</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Speedometer Drive</para>
                            </entry>
                            <entry>
                                <para>Mechanical or electronic</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Power Take-Off</para>
                            </entry>
                            <entry>
                                <para>SAE standard 6 bolt, right and left; countershaft rear optional</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>PTO Drive Speed</para>
                            </entry>
                            <entry>
                                <para>13DBA, 13DBC: 48% engine speed, 6-pitch, 34 teeth, 13DBB, 13DBD: 48% engine speed,
                                    6-pitch, 32 teeth
                                </para>
                            </entry>
                        </row>
                    </tbody>
                </tgroup>
            </table>
        </svcsubsecinfo>

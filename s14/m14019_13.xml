                <title>Prueba de funcionamiento</title>
                <tgroup cols="6">
                    <colspec colname="col1" colwidth="0.55in"/>
                    <colspec colname="col2" colwidth="0.59in"/>
                    <colspec colname="col3" colwidth="1.65in"/>
                    <colspec colname="col5" colwidth="1.69in"/>
                    <colspec colname="col6" colwidth="0.91in"/>
                    <colspec colname="col7" colwidth="1.30in"/>

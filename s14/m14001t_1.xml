                            <callout1>2. TUERCA DE AJUSTE DEL BALERO EXTERIOR DE LA RUEDA</callout1>
                            <callout1>3. CONTRATUERCA DE AJUSTE DEL BALERO DE LA RUEDA</callout1>
                            <callout1>4. TUERCA DE AJUSTE DEL BALERO INTERIOR DE LA RUEDA</callout1>
                            <callout1>5. BALERO EXTERIOR</callout1>
                            <callout1>6. BALERO EXTERIOR DE LA COPA</callout1>
                            <callout1>7. PERNO DE REBORDE DEL SEMIEJE</callout1>
                            <callout1>8. CUBO</callout1>
                            <callout1>9. BALERO INTERIOR DE LA COPA</callout1>
                            <callout1>10. BALERO INTERIOR</callout1>
                            <callout1>11. SELLO O ACEITE</callout1>

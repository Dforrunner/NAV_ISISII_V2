<isis lang="en">
    <stitle>ELECTRICAL SYSTEM TROUBLESHOOTING GUIDE - 9800 - LIFT AXLE SYSTEMS - SPECIFICATIONS</stitle>
    <svcsection/>
    <svcsubsec id="hl14019080">
        <title>SPECIFICATIONS</title>
        <svcproc id="hl14019081" svcproctype="describe" graphiccount="0">
            <title>LIFT AXLE SYSTEM SPECIFICATIONS</title>
            <table id="t14019n" tblwidth="full-page">
                <title>Lift Axle System Specifications - 20,000 Pound Capacity Rigid Axle Assembly</title>
                <tgroup cols="2" colsep="1" rowsep="0">
                    <colspec colname="1" colwidth="3.49in"/>
                    <colspec colname="2" colwidth="2.68in"/>
                    <spanspec namest="1" nameend="2" spanname="1to2"/>
                    <thead>
                        <row>
                            <entry colname="1" align="center">
                                <para>
                                    <emph type="bold">ITEM</emph>
                                </para>
                            </entry>
                            <entry colname="2" align="center">
                                <para>
                                    <emph type="bold">SPECIFICATION</emph>
                                </para>
                            </entry>
                        </row>
                    </thead>
                    <tbody>
                        <row>
                            <entry colname="1">
                                <para>Maximum Load Capacity</para>
                            </entry>
                            <entry colname="2">
                                <para>20,000 pound</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>Air system</para>
                            </entry>
                            <entry colname="2">
                                <para>integral axle mounted air tank fed by wet tank</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Axle Connection</para>
                            </entry>
                            <entry>
                                <para>two pin, rubber bushed axle connection</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Axle Travel</para>
                            </entry>
                            <entry>
                                <para>10 inches of axle travel (7 up/3 down)</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>Wheel Ends, Type</para>
                            </entry>
                            <entry>
                                <para>cast hub-piloted</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>Hubs and Drums</para>
                            </entry>
                            <entry colname="2">
                                <para>Standard hubs and drums are cast 10 hole - 11.25in bc Unimount.</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>Brakes System</para>
                            </entry>
                            <entry colname="2">
                                <para>Standard brakes system includes 16.5X7 Meritor Q-Plus brakes, Type 30 MGM
                                    chambers, 931 - 162 Federal Mogul brake linings, and a Meritor slack adjuster.
                                </para>
                            </entry>
                        </row>
                    </tbody>
                </tgroup>
            </table>
            <table id="t14019p" tblwidth="full-page">
                <title>Lift Axle System Specifications - 13,200 Pound Capacity Self-Steering Axle Assembly</title>
                <tgroup cols="2" colsep="1" rowsep="0">
                    <colspec colname="1" colwidth="3.49in"/>
                    <colspec colname="2" colwidth="2.68in"/>
                    <spanspec namest="1" nameend="2" spanname="1to2"/>
                    <thead>
                        <row>
                            <entry colname="1" align="center">
                                <para>
                                    <emph type="bold">ITEM</emph>
                                </para>
                            </entry>
                            <entry colname="2" align="center">
                                <para>
                                    <emph type="bold">SPECIFICATION</emph>
                                </para>
                            </entry>
                        </row>
                    </thead>
                    <tbody>
                        <row>
                            <entry colname="1">
                                <para>Maximum Load Capacity</para>
                            </entry>
                            <entry colname="2">
                                <para>13,200 pound</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>Air System</para>
                            </entry>
                            <entry colname="2">
                                <para>integral axle mounted air tank fed by wet tank</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>Axle Type</para>
                            </entry>
                            <entry colname="2">
                                <para>fabricated</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>Axle Connection</para>
                            </entry>
                            <entry colname="2">
                                <para>two pin, rubber bushed axle connection</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>Axle Travel</para>
                            </entry>
                            <entry colname="2">
                                <para>13.5 inches</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>Wheel Ends, Type</para>
                            </entry>
                            <entry colname="2">
                                <para>cast hub-piloted</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>Hubs and Drums</para>
                            </entry>
                            <entry colname="2">
                                <para>Standard hubs and drums are cast 10 hole - 11.25in bc Unimount.</para>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>Steering Mechanism</para>
                            </entry>
                            <entry colname="2">
                                <randlist type="bullet">
                                    <item>
                                        <para>dual self-centering stabilizers</para>
                                    </item>
                                    <item>
                                        <para>preset caster</para>
                                    </item>
                                </randlist>
                            </entry>
                        </row>
                        <row>
                            <entry colname="1">
                                <para>Brakes System</para>
                            </entry>
                            <entry colname="2">
                                <para>Standard brakes system includes 15X4 Meritor Q-Plus brakes, Type 20 MGM chambers,
                                    931 - 162 Federal Mogul brake linings, and a Meritor slack adjuster.
                                </para>
                            </entry>
                        </row>
                    </tbody>
                </tgroup>
            </table>

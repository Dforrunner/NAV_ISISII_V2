<isis lang="en">
    <stitle>ELECTRICAL SYSTEM TROUBLESHOOTING GUIDE - 9800 - CAB - TILT: 9600 AND 9700 SLEEPER AND NON-SLEEPER CABS -
        TROUBLESHOOTING WINDSHIELD WASHER SYSTEM
    </stitle>
    <svcsection/>
    <svcproc svcproctype="troubleshoot" graphiccount="0">
        <title>TROUBLESHOOTING WINDSHIELD WASHER SYSTEM</title>
        <table id="hl1600629">
            <title>Troubleshooting Windshield Washer System</title>
            <tgroup cols="3">
                <colspec colname="1" colwidth="1.49in"/>
                <colspec colname="2" colwidth="2.74in"/>
                <colspec colname="3" colwidth="2.74in"/>
                <thead>
                    <row rowsep="0">
                        <entry colname="1">
                            <para>Condition</para>
                        </entry>
                        <entry colname="2">
                            <para>Probable Cause</para>
                        </entry>
                        <entry colname="3">
                            <para>Remedy</para>
                        </entry>
                    </row>
                </thead>
                <tbody>
                    <row rowsep="0">
                        <entry colname="1">
                            <para>Pump will not operate</para>
                        </entry>
                        <entry colname="2">
                            <para>No voltage to pump-motor</para>
                        </entry>
                        <entry colname="3">
                            <para>Check for open circuit or blown fuse.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry colname="1">
                            <para></para>
                        </entry>
                        <entry colname="2">
                            <para>Defective control unit.</para>
                        </entry>
                        <entry colname="3">
                            <para>Replace control unit.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry colname="1">
                            <para></para>
                        </entry>
                        <entry colname="2">
                            <para>Defective motor.</para>
                        </entry>
                        <entry colname="3">
                            <para>Replace pump-motor reservoir assembly.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry colname="1">
                            <para>Pump operates, but pressure is weak</para>
                        </entry>
                        <entry colname="2">
                            <para>Hoses loose, kinked or damaged.</para>
                        </entry>
                        <entry colname="3">
                            <para>Reinstall or replace hose.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry colname="1">
                            <para></para>
                        </entry>
                        <entry colname="2">
                            <para>Nozzles plugged or dirt in reservoir.</para>
                        </entry>
                        <entry colname="3">
                            <para>Clean nozzles and service reservoir as required.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry colname="1">
                            <para></para>
                        </entry>
                        <entry colname="2">
                            <para>Defective motor or pump.</para>
                        </entry>
                        <entry colname="3">
                            <para>Replace pump-motor reservoir assembly.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry colname="1">
                            <para>Pump operates but no washer solution delivered</para>
                        </entry>
                        <entry colname="2">
                            <para>No solution in reservoir.</para>
                        </entry>
                        <entry colname="3">
                            <para>Fill reservoir.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry colname="1">
                            <para></para>
                        </entry>
                        <entry colname="2">
                            <para>Hoses loose, kinked or damaged.</para>
                        </entry>
                        <entry colname="3">
                            <para>Reinstall or replace hoses</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry colname="1">
                            <para></para>
                        </entry>
                        <entry colname="2">
                            <para>Nozzle plugged or dirt in reservoir.</para>
                        </entry>
                        <entry colname="3">
                            <para>Clean nozzles or reservoir as required.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry colname="1">
                            <para></para>
                        </entry>
                        <entry colname="2">
                            <para>Solution frozen</para>
                        </entry>
                        <entry colname="3">
                            <para>Thaw out system and replace washer solvent.</para>
                        </entry>
                    </row>
                    <row rowsep="0">
                        <entry colname="1">
                            <para></para>
                        </entry>
                        <entry colname="2">
                            <para>Defective pump.</para>
                        </entry>
                        <entry colname="3">
                            <para>Replace pump motor reservoir assembly.</para>
                        </entry>
                    </row>
                </tbody>
            </tgroup>
        </table>
    </svcproc>

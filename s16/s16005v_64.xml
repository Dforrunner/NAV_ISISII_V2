<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../isisxsl.xsl"?>
<isis lang="en">
    <stitle>ELECTRICAL SYSTEM TROUBLESHOOTING GUIDE - 9800 - CAB: 5000, 9100, 9200, 9300, 9400 - TROUBLESHOOTING
    </stitle>
    <svcsection/>
    <svcsubsec>
        <title>TROUBLESHOOTING</title>
        <svcproc svcproctype="troubleshoot" graphiccount="0">
            <title>ELECTRIC WIPERS</title>
            <table id="t1600501">
                <title>Electric Wiper Troubleshooting Chart</title>
                <tgroup cols="3">
                    <colspec colnum="1" colname="sqcolumn1" colwidth="1.5in"/>
                    <colspec colnum="2" colname="sqcolumn2" colwidth="2.5in"/>
                    <colspec colnum="3" colname="sqcolumn3" colwidth="2in" colsep="1"/>
                    <thead valign="top">
                        <row>
                            <entry>
                                <para>
                                    <emph type="bold">Condition</emph>
                                </para>
                            </entry>
                            <entry>
                                <para>
                                    <emph type="bold">Probable Cause</emph>
                                </para>
                            </entry>
                            <entry>
                                <para>
                                    <emph type="bold">Remedy</emph>
                                </para>
                            </entry>
                        </row>
                    </thead>
                    <tbody valign="top">
                        <row>
                            <entry>
                                <para>Stripped gear teeth</para>
                            </entry>
                            <entry>
                                <para>Normal wear/ frozen wiper arm.</para>
                            </entry>
                            <entry>
                                <para>Replace complete unit.</para>
                            </entry>
                        </row>
                        <row>
                            <entry morerows="4">
                                <para>Wiper will not operate</para>
                            </entry>
                            <entry>
                                <para>Binding condition in wiper arm shaft assemblies, connecting links, gear and shaft
                                    assembly or armature.
                                </para>
                            </entry>
                            <entry>
                                <para>Eliminate binding condition. If gear or shaft, replace motor assembly.</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>-</para>
                            </entry>
                            <entry colname="sqcolumn2">
                                <para>Short, open, or ground in cable harness.</para>
                            </entry>
                            <entry colname="sqcolumn3">
                                <para>Refer to Electrical Troubleshooting Manual.</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>-</para>
                            </entry>
                            <entry colname="sqcolumn2">
                                <para>Faulty manual switch.</para>
                            </entry>
                            <entry colname="sqcolumn3">
                                <para>Replace switch.</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>-</para>
                            </entry>
                            <entry colname="sqcolumn2">
                                <para>Shorted, open, or grounded armature.</para>
                            </entry>
                            <entry colname="sqcolumn3">
                                <para>Replace motor assembly.</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>-</para>
                            </entry>
                            <entry colname="sqcolumn2">
                                <para>Broken wire to motor assembly.</para>
                            </entry>
                            <entry colname="sqcolumn3">
                                <para>Correct.</para>
                            </entry>
                        </row>
                        <row>
                            <entry morerows="1">
                                <para>Wiper will not shut off</para>
                            </entry>
                            <entry>
                                <para>Faulty parking switch assembly.</para>
                            </entry>
                            <entry>
                                <para>Replace complete unit.</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>-</para>
                            </entry>
                            <entry colname="sqcolumn2">
                                <para>Faulty manual switch.</para>
                            </entry>
                            <entry colname="sqcolumn3">
                                <para>Replace manual switch.</para>
                            </entry>
                        </row>
                        <row>
                            <entry morerows="2">
                                <para>Wiper continually shutting off</para>
                            </entry>
                            <entry>
                                <para>Binding condition in wiper arm shaft assemblies, connecting link, or gear and
                                    shaft assembly.
                                </para>
                            </entry>
                            <entry>
                                <para>Eliminate binding condition.</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>-</para>
                            </entry>
                            <entry colname="sqcolumn2">
                                <para>Electrical overload due to faulty motor.</para>
                            </entry>
                            <entry colname="sqcolumn3">
                                <para>Replace motor assembly.</para>
                            </entry>
                        </row>
                        <row>
                            <entry>
                                <para>-</para>
                            </entry>
                            <entry colname="sqcolumn2">
                                <para>Faulty wire connections.</para>
                            </entry>
                            <entry colname="sqcolumn3">
                                <para>Correct.</para>
                            </entry>
                        </row>
                        <row>
                            <entry morerows="1">
                                <para>Wiper operates at one speed only</para>
                            </entry>
                            <entry>
                                <para>Short, open, or ground in cable harness.</para>
                            </entry>
                            <entry>
                                <para>Refer to Electrical Troubleshooting Manual.</para>
                            </entry>
                        </row>
                        <row rowsep="1">
                            <entry>
                                <para>-</para>
                            </entry>
                            <entry colname="sqcolumn2">
                                <para>Faulty manual switch.</para>
                            </entry>
                            <entry colname="sqcolumn3">
                                <para>Replace switch.</para>
                            </entry>
                        </row>
                    </tbody>
                </tgroup>
            </table>
        </svcproc>
    </svcsubsec>

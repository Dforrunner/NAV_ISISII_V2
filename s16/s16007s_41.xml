<isis lang="en">
    <stitle>CAB: 2000, 4000, AND 8000 - TROUBLESHOOTING - WASHER PUMP MOTOR</stitle>
    <svcsection/>
    <svcproc svcproctype="IDcomponent" graphiccount="0">
        <title>WASHER PUMP MOTOR</title>
        <table id="t1600702" tblwidth="full-page" frame="all" numbered="no" view="reveal">
            <title>Washer Pump Motor</title>
            <tgroup cols="3" align="center" charoff="50" char="c">
                <colspec colname="1" colwidth="1.56in"/>
                <colspec colname="2" colwidth="2.46in"/>
                <colspec colname="3" colwidth="2.60in"/>
                <thead>
                    <row>
                        <entry colname="1">
                            <para>
                                <emph type="bold">CONDITION</emph>
                            </para>
                        </entry>
                        <entry colname="2">
                            <para>
                                <emph type="bold">PROBABLE CAUSE</emph>
                            </para>
                        </entry>
                        <entry colname="3">
                            <para>
                                <emph type="bold">REMEDY</emph>
                            </para>
                        </entry>
                    </row>
                </thead>
                <tbody valign="top">
                    <row>
                        <entry colname="1">
                            <para>Pump will not</para>
                            <para>
                                <emph type="bold">operate</emph>
                            </para>
                        </entry>
                        <entry colname="2">
                            <seqlist style="alphauc">
                                <item>
                                    <para>No voltage to pump motor.</para>
                                </item>
                                <item>
                                    <para>Defective switch.</para>
                                </item>
                                <item>
                                    <para>Defective pump motor.</para>
                                </item>
                            </seqlist>
                        </entry>
                        <entry colname="3">
                            <seqlist style="alphauc">
                                <item>
                                    <para>Check for open circuit or blown fuse.</para>
                                </item>
                                <item>
                                    <para>Replace switch.</para>
                                </item>
                                <item>
                                    <para>Replace pump/motor assembly.</para>
                                </item>
                            </seqlist>
                        </entry>
                    </row>
                    <row>
                        <entry colname="1">
                            <para>Pump operates but pressure is weak</para>
                        </entry>
                        <entry colname="2">
                            <seqlist style="alphauc">
                                <item>
                                    <para>Hoses loose, kinked or damaged.</para>
                                </item>
                                <item>
                                    <para>Nozzles plugged or dirt in reservoir.</para>
                                </item>
                                <item>
                                    <para>Defective motor or pump.</para>
                                </item>
                            </seqlist>
                        </entry>
                        <entry colname="3">
                            <seqlist style="alphauc">
                                <item>
                                    <para>Reinstall or replace hose.</para>
                                </item>
                                <item>
                                    <para>Clean nozzles and service reservoir as required.</para>
                                </item>
                                <item>
                                    <para>Replace pump/motor assembly.</para>
                                </item>
                            </seqlist>
                        </entry>
                    </row>
                    <row>
                        <entry colname="1">
                            <para>Pump operates but no washer solution delivered.</para>
                        </entry>
                        <entry colname="2">
                            <seqlist style="alphauc">
                                <item>
                                    <para>No solution in reservoir.</para>
                                </item>
                                <item>
                                    <para>Hoses loose, kinked or damaged.</para>
                                </item>
                                <item>
                                    <para>Nozzles plugged or dirt in reservoir.</para>
                                </item>
                                <item>
                                    <para>Solution frozen.</para>
                                </item>
                                <item>
                                    <para>Defective pump.</para>
                                </item>
                            </seqlist>
                        </entry>
                        <entry colname="3">
                            <seqlist style="alphauc">
                                <item>
                                    <para>Fill reservoir.</para>
                                </item>
                                <item>
                                    <para>Reinstall or replace hoses.</para>
                                </item>
                                <item>
                                    <para>Clean nozzles or reservoir as required.</para>
                                </item>
                                <item>
                                    <para>Thaw out system and replace washer solvent.</para>
                                </item>
                                <item>
                                    <para>Replace pump/motor assembly.</para>
                                </item>
                            </seqlist>
                        </entry>
                    </row>
                </tbody>
            </tgroup>
        </table>
    </svcproc>

<isis lang="en">
    <stitle>ELECTRICAL SYSTEM TROUBLESHOOTING GUIDE - 9800 - CAB - TILT: 9800 - TROUBLESHOOTING - WINDSHIELD WASHER
        SYSTEM - ELECTRIC
    </stitle>
    <svcsection/>
    <svcproc svcproctype="troubleshoot" graphiccount="0">
        <title>WINDSHIELD WASHER SYSTEM - ELECTRIC</title>
        <table id="t1601802">
            <title>Electric Windshield Washer System Troubleshooting</title>
            <tgroup cols="3">
                <colspec colnum="1" colname="sqcolumn1" colwidth="1.68in"/>
                <colspec colnum="2" colname="sqcolumn2" colwidth="2.50in"/>
                <colspec colnum="3" colname="sqcolumn3" colwidth="2.50in" colsep="1"/>
                <thead valign="top">
                    <row>
                        <entry>
                            <para>Condition</para>
                        </entry>
                        <entry>
                            <para>Probable Cause</para>
                        </entry>
                        <entry>
                            <para>Remedy</para>
                        </entry>
                    </row>
                </thead>
                <tbody valign="top">
                    <row>
                        <entry morerows="2">
                            <para>Pump will not operate</para>
                        </entry>
                        <entry>
                            <para>No voltage to pump motor.</para>
                        </entry>
                        <entry>
                            <para>Check for open circuit or blown fuse.</para>
                        </entry>
                    </row>
                    <row>
                        <entry>
                            <para>-</para>
                        </entry>
                        <entry colname="sqcolumn2">
                            <para>Defective control unit.</para>
                        </entry>
                        <entry colname="sqcolumn3">
                            <para>Replace control unit.</para>
                        </entry>
                    </row>
                    <row>
                        <entry>
                            <para>-</para>
                        </entry>
                        <entry colname="sqcolumn2">
                            <para>Defective pump motor.</para>
                        </entry>
                        <entry colname="sqcolumn3">
                            <para>Replace pump motor/reservoir assembly.</para>
                        </entry>
                    </row>
                    <row>
                        <entry morerows="2">
                            <para>Pump operates, but pressure is weak</para>
                        </entry>
                        <entry>
                            <para>Hoses loose, kinked or damaged.</para>
                        </entry>
                        <entry>
                            <para>Re-install or replace hose.</para>
                        </entry>
                    </row>
                    <row>
                        <entry>
                            <para>-</para>
                        </entry>
                        <entry colname="sqcolumn2">
                            <para>Nozzles plugged or dirt in reservoir.</para>
                        </entry>
                        <entry colname="sqcolumn3">
                            <para>Clean nozzles and service reservoir as required.</para>
                        </entry>
                    </row>
                    <row>
                        <entry>
                            <para>-</para>
                        </entry>
                        <entry colname="sqcolumn2">
                            <para>Defective motor or pump.</para>
                        </entry>
                        <entry colname="sqcolumn3">
                            <para>Replace pump motor/reservoir assembly.</para>
                        </entry>
                    </row>
                    <row>
                        <entry morerows="4">
                            <para>Pump operates but no washer solution delivered</para>
                        </entry>
                        <entry>
                            <para>No solution in reservoir.</para>
                        </entry>
                        <entry>
                            <para>Fill reservoir.</para>
                        </entry>
                    </row>
                    <row>
                        <entry>
                            <para>-</para>
                        </entry>
                        <entry colname="sqcolumn2">
                            <para>Hoses loose, kinked or damaged.</para>
                        </entry>
                        <entry colname="sqcolumn3">
                            <para>Re-install or replace hose.</para>
                        </entry>
                    </row>
                    <row>
                        <entry>
                            <para>-</para>
                        </entry>
                        <entry colname="sqcolumn2">
                            <para>Nozzle plugged or dirt in reservoir.</para>
                        </entry>
                        <entry colname="sqcolumn3">
                            <para>Clean nozzles or reservoir as required.</para>
                        </entry>
                    </row>
                    <row>
                        <entry>
                            <para>-</para>
                        </entry>
                        <entry colname="sqcolumn2">
                            <para>Solution frozen.</para>
                        </entry>
                        <entry colname="sqcolumn3">
                            <para>Thaw out system and replace washer solvent.</para>
                        </entry>
                    </row>
                    <row rowsep="1">
                        <entry>
                            <para>-</para>
                        </entry>
                        <entry colname="sqcolumn2">
                            <para>Defective pump motor.</para>
                        </entry>
                        <entry colname="sqcolumn3">
                            <para>Replace pump motor/reservoir assembly</para>
                        </entry>
                    </row>
                </tbody>
            </tgroup>
        </table>
    </svcproc>

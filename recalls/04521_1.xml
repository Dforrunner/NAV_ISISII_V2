<isis lang="en">
    <stitle>Recall 04521 Interim Action - CAB MOUNTS on certain 4000, 7000, and 8000 Series Models built 1/16/2004 thru
        11/1/2004
    </stitle>
    <recall id="rec04521">
        <recallnumber>04521 Interim Action</recallnumber>
        <market>U.S., Export</market>
        <recalldate>February, 2005</recalldate>
        <subjectfile>CAB</subjectfile>
        <subject>CAB MOUNTS on certain 4000, 7000, and 8000 Series Models built 1/16/2004 thru 11/1/2004</subject>
